roundStyle(_:)"},"schemaVersion":{"major":0,"patch":0,"minor":3},"primaryContentSections":[{"declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"kind":"attribute","text":"nonisolated"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"foregroundStyle"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"S"},{"kind":"text","text":">("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"style"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"S"},{"kind":"text","text":") -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP","kind":"typeIdentifier","text":"View"},{"kind":"text","text":" "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"S"},{"kind":"text","text":" : "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle","preciseIdentifier":"s:15OpenSwiftUICore10ShapeStyleP","kind":"typeIdentifier","text":"ShapeStyle"},{"kind":"text","text":"\n"}]}],"kind":"declarations"},{"parameters":[{"name":"style","content":[{"inlineContent":[{"type":"text","text":"The color or pattern to use when filling in the"},{"type":"text","text":" "},{"type":"text","text":"foreground elements. To indicate a specific value, use "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Color"},{"type":"text","text":" or"},{"type":"text","text":" "},{"type":"codeVoice","code":"ShapeStyle\/image(_:sourceRect:scale:)"},{"type":"text","text":", or one of the gradient"},{"type":"text","text":" "},{"type":"text","text":"types, like"},{"type":"text","text":" "},{"type":"codeVoice","code":"ShapeStyle\/linearGradient(colors:startPoint:endPoint:)"},{"type":"text","text":". To set a"},{"type":"text","text":" "},{"type":"text","text":"style that’s relative to the containing view’s style, use one of the"},{"type":"text","text":" "},{"type":"text","text":"semantic styles, like "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/primary"},{"type":"text","text":"."}],"type":"paragraph"}]}],"kind":"parameters"},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"return-value","text":"Return Value"},{"type":"paragraph","inlineContent":[{"text":"A view that uses the given foreground style.","type":"text"}]}]},{"kind":"content","content":[{"anchor":"discussion","type":"heading","text":"Discussion","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"Use this method to style"},{"type":"text","text":" "},{"type":"text","text":"foreground content like text, shapes, and template images"},{"type":"text","text":" "},{"type":"text","text":"(including symbols):"}]},{"code":["HStack {","    Image(systemName: \"triangle.fill\")","    Text(\"Hello, world!\")","    RoundedRectangle(cornerRadius: 5)","        .frame(width: 40, height: 20)","}",".foregroundStyle(.teal)"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The example above creates a row of "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/teal","isActive":true},{"type":"text","text":" foreground"},{"type":"text","text":" "},{"type":"text","text":"elements:"}]},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"text":"You can use any style that conforms to the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle","isActive":true,"type":"reference"},{"text":" protocol,","type":"text"},{"text":" ","type":"text"},{"text":"like the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/teal","isActive":true,"type":"reference"},{"text":" color in the example above, or the","type":"text"},{"text":" ","type":"text"},{"code":"ShapeStyle\/linearGradient(colors:startPoint:endPoint:)","type":"codeVoice"},{"text":" gradient","type":"text"},{"text":" ","type":"text"},{"text":"shown below:","type":"text"}]},{"code":["Text(\"Gradient Text\")","    .font(.largeTitle)","    .foregroundStyle(","        .linearGradient(","            colors: [.yellow, .blue],","            startPoint: .top,","            endPoint: .bottom","        )","    )"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[]},{"style":"tip","type":"aside","name":"Tip","content":[{"inlineContent":[{"text":"If you want to fill a single ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Shape","isActive":true,"type":"reference"},{"text":" instance with a style,","type":"text"},{"text":" ","type":"text"},{"text":"use the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Shape\/fill(style:)","isActive":true,"type":"reference"},{"text":" shape modifier instead because it’s more","type":"text"},{"text":" ","type":"text"},{"text":"efficient.","type":"text"}],"type":"paragraph"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"OpenSwiftUI creates a context-dependent render for a given style."},{"type":"text","text":" "},{"type":"text","text":"For example, a "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Color","isActive":true,"type":"reference"},{"type":"text","text":" that you load from an asset catalog"},{"type":"text","text":" "},{"type":"text","text":"can have different light and dark appearances, while some styles"},{"type":"text","text":" "},{"type":"text","text":"also vary by platform."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Hierarchical foreground styles like "},{"type":"codeVoice","code":"ShapeStyle\/secondary"},{"type":"text","text":" "},{"type":"text","text":"don’t impose a style of their own, but instead modify other styles."},{"type":"text","text":" "},{"type":"text","text":"In particular, they modify the primary"},{"type":"text","text":" "},{"type":"text","text":"level of the current foreground style to the degree given by"},{"type":"text","text":" "},{"type":"text","text":"the hierarchical style’s name."},{"type":"text","text":" "},{"type":"text","text":"To find the current foreground style to modify, OpenSwiftUI looks for"},{"type":"text","text":" "},{"type":"text","text":"the innermost containing style that you apply with the"},{"type":"text","text":" "},{"type":"codeVoice","code":"foregroundStyle(_:)"},{"type":"text","text":" or the "},{"type":"codeVoice","code":"View\/foregroundColor(_:)"},{"type":"text","text":" modifier."},{"type":"text","text":" "},{"type":"text","text":"If you haven’t specified a style, OpenSwiftUI uses the default foreground"},{"type":"text","text":" "},{"type":"text","text":"style, as in the following example:"}]},{"code":["VStack(alignment: .leading) {","    Label(\"Primary\", systemImage: \"1.square.fill\")","    Label(\"Secondary\", systemImage: \"2.square.fill\")","        .foregroundStyle(.secondary)","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If you add a foreground style on the enclosing"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack"},{"type":"text","text":", the hierarchical styling responds accordingly:"}]},{"code":["VStack(alignment: .leading) {","    Label(\"Primary\", systemImage: \"1.square.fill\")","    Label(\"Secondary\", systemImage: \"2.square.fill\")","        .foregroundStyle(.secondary)","}",".foregroundStyle(.blue)"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"type":"text","text":"When you apply a custom style to a view, the view disables the vibrancy"},{"type":"text","text":" "},{"type":"text","text":"effect for foreground elements in that view, or in any of its child"},{"type":"text","text":" "},{"type":"text","text":"views, that it would otherwise gain from adding a background material"},{"type":"text","text":" "},{"type":"text","text":"— for example, using the "},{"type":"codeVoice","code":"View\/background(_:ignoresSafeAreaEdges:)"},{"type":"text","text":" "},{"type":"text","text":"modifier. However, hierarchical styles applied to the default foreground"},{"type":"text","text":" "},{"type":"text","text":"don’t disable vibrancy."}]}]}],"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/RoundedRectangle":{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/RoundedRectangle","kind":"symbol","type":"topic","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"RoundedRectangle","kind":"identifier"}],"title":"RoundedRectangle","navigatorTitle":[{"text":"RoundedRectangle","kind":"identifier"}],"url":"\/documentation\/openswiftui\/roundedrectangle","abstract":[{"text":"A rectangular shape with rounded corners, aligned inside the frame of the","type":"text"},{"text":" ","type":"text"},{"text":"view containing it.","type":"text"}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/Color":{"type":"topic","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Color","abstract":[{"type":"text","text":"A representation of a color that adapts to a given context."}],"url":"\/documentation\/openswiftui\/color","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"Color","kind":"identifier"}],"kind":"symbol","role":"symbol","title":"Color","navigatorTitle":[{"text":"Color","kind":"identifier"}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/ShapeStyle/primary":{"title":"primary","abstract":[{"type":"text","text":"A shape style that maps to the first level of the current content style."}],"url":"\/documentation\/openswiftui\/shapestyle\/pri