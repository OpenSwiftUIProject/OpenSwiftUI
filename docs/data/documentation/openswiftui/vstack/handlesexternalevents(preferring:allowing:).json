er":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack\/handlesExternalEvents(preferring:allowing:)","role":"symbol","kind":"symbol","url":"\/documentation\/openswiftui\/vstack\/handlesexternalevents(preferring:allowing:)"},"https://developer.apple.com/documentation/foundation/nsuseractivity":{"url":"https:\/\/developer.apple.com\/documentation\/foundation\/nsuseractivity","type":"link","title":"NSUserActivity","identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/nsuseractivity","titleInlineContent":[{"type":"text","text":"NSUserActivity"}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/Scene/handlesExternalEvents(matching:)":{"title":"handlesExternalEvents(matching:)","abstract":[{"type":"text","text":"Specifies the external events for which OpenSwiftUI opens a new instance"},{"type":"text","text":" "},{"type":"text","text":"of the modified scene."}],"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Scene\/handlesExternalEvents(matching:)","kind":"symbol","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"handlesExternalEvents","kind":"identifier"},{"text":"(","kind":"text"},{"text":"matching","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"Set","kind":"typeIdentifier","preciseIdentifier":"s:Sh"},{"text":"<","kind":"text"},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"},{"text":">) -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"text":"Scene","kind":"typeIdentifier","preciseIdentifier":"s:11OpenSwiftUI5SceneP"}],"url":"\/documentation\/openswiftui\/scene\/handlesexternalevents(matching:)","role":"symbol","type":"topic"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/VStack":{"kind":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"VStack"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"VStack"}],"type":"topic","title":"VStack","abstract":[{"type":"text","