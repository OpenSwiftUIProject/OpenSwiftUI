hOS","deprecated":false,"beta":false}],"roleHeading":"Instance Method","role":"symbol","symbolKind":"method"},"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"nonisolated","kind":"attribute"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"tag","kind":"identifier"},{"text":"<","kind":"text"},{"text":"V","kind":"genericParameter"},{"text":">(","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"tag","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"includeOptional","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"Bool","preciseIdentifier":"s:Sb","kind":"typeIdentifier"},{"text":" = true) -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP","kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"text":" ","kind":"text"},{"text":"where","kind":"keyword"},{"text":" ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"Hashable","preciseIdentifier":"s:SH","kind":"typeIdentifier"},{"text":"\n","kind":"text"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"parameters","parameters":[{"name":"tag","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A "},{"isActive":true,"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable"},{"type":"text","text":" "},{"type":"text","text":"value to use as the view’s tag."}]}]},{"name":"includeOptional","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"If the tag value for "},{"type":"codeVoice","code":"Optional<V>"},{"type":"text","text":" should"},{"type":"text","text":" "},{"type":"text","text":"also be set."}]}]}]},{"kind":"content","content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"text":"A view with the specified tag set.","type":"text"}]}]},{"kind":"content","content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"inlineContent":[{"text":"Use this modifier to differentiate among certain selectable views,","type":"text"},{"text":" ","type":"text"},{"text":"like the possible values of a ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" or the tabs of a ","type":"text"},{"code":"TabView","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"Tag values can be of any type that conforms to the","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","isActive":true},{"text":" protocol.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"This modifier will write the tag value for the type "},{"type":"codeVoice","code":"V"},{"type":"text","text":", as well as"},{"type":"text","text":" "},{"type":"codeVoice","code":"Optional<V>"},{"type":"text","text":" if "},{"type":"codeVoice","code":"includeOptional"},{"type":"text","text":" is enabled. Containers checking for"},{"type":"text","text":" "},{"type":"text","text":"tags of either type will see the value as set."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"In the example below, the "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true},{"type":"text","text":" loop in the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":" view"},{"type":"text","text":" "},{"type":"text","text":"builder iterates over the "},{"type":"codeVoice","code":"Flavor"},{"type":"text","text":" enumeration. It extracts the string"},{"type":"text","text":" "},{"type":"text","text":"value of each enumeration element for use in constructing the row"},{"type":"text","text":" "},{"type":"text","text":"label, and uses the enumeration value as input to the "},{"type":"codeVoice","code":"tag(_:)"},{"type":"text","text":" "},{"type":"text","text":"modifier."}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["struct FlavorPicker: View {","    enum Flavor: String, CaseIterable, Identifiable {","        case chocolate, vanilla, strawberry","        var id: Self { self }","    }","","    @State private var selectedFlavor: Flavor? = nil","","    var body: some View {","        Picker(\"Flavor\", selection: $selectedFlavor) {","            ForEach(Flavor.allCases) { flavor in","                Text(flavor.rawValue)","                    .tag(flavor)","            }","        }","    }","}"]},{"inlineContent":[{"type":"text","text":"The selection type of the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":" is an "},{"type":"codeVoice","code":"Optional<Flavor>"},{"type":"text","text":" and so it"},{"type":"text","text":" "},{"type":"text","text":"will look for tags on its contents of "},{"type":"codeVoice","code":"Optional<Flavor>"},{"type":"text","text":"type. Since the"},{"type":"text","text":" "},{"type":"text","text":"tag modifier defaults to having "},{"type":"codeVoice","code":"includeOptional"},{"type":"text","text":" enabled, even though"},{"type":"text","text":" "},{"type":"text","text":"the tag for each option is a non-optional "},{"type":"codeVoice","code":"Flavor"},{"type":"text","text":", the tag modifier"},{"type":"text","text":" "},{"type":"text","text":"writes values for both the non-optional, and optional versions of the"},{"type":"text","text":" "},{"type":"text","text":"value, allowing the contents to be selectable by the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"text":"A ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true,"type":"reference"},{"text":" automatically applies a default tag to each enumerated","type":"text"},{"text":" ","type":"text"},{"text":"view using the ","type":"text"},{"code":"id","type":"codeVoice"},{"text":" parameter of the corresponding element. If","type":"text"},{"text":" ","type":"text"},{"text":"the element’s ","type":"text"},{"code":"id","type":"codeVoice"},{"text":" parameter and the picker’s ","type":"text"},{"code":"selection","type":"codeVoice"},{"text":" input","type":"text"},{"text":" ","type":"text"},{"text":"have exactly the same type, or the same type but optional, you can omit","type":"text"},{"text":" ","type":"text"},{"text":"the explicit tag modifier.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"To see examples that don’t require an explicit tag, see ","type":"text"},{"type":"codeVoice","code":"Picker"},{"text":".","type":"text"}],"type":"paragraph"}]}],"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/VStack":{"kind":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"VStack"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"VStack"}],"type":"topic","title":"VStack","abstract":[{"type":"text","text":"A view that arranges its subviews in a vertical line."}],"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack","url":"\/documentation\/openswiftui\/vstack"},"doc://OpenSwiftUI/documentation/OpenSwiftUI":{"type":"topic","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","url":"\/documentation\/openswiftui","title":"OpenSwiftUI","kind":"symbol","role":"collection","abstract":[{"type":"text","text":"Declare the user interface and behavior for your app on every platform."}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/View":{"title":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","type":"topic","kind":"symbol","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","kind":"identifier"}],"role":"symbol","url":"\/docume