{"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PrimitiveButtonStyleConfiguration\/Label-swift.struct\/foregroundStyle(_:)"},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/openswiftui\/primitivebuttonstyleconfiguration\/label-swift.struct\/foregroundstyle(_:)"]}],"abstract":[{"text":"Sets a view’s foreground elements to use a given style.","type":"text"}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"nonisolated","kind":"attribute"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"foregroundStyle","kind":"identifier"},{"text":"<","kind":"text"},{"text":"S","kind":"genericParameter"},{"text":">(","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"style","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"S","kind":"typeIdentifier"},{"text":") -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"preciseIdentifier":"s:15OpenSwiftUICore4ViewP","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","text":"View","kind":"typeIdentifier"},{"text":" ","kind":"text"},{"text":"where","kind":"keyword"},{"text":" ","kind":"text"},{"text":"S","kind":"typeIdentifier"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:15OpenSwiftUICore10ShapeStyleP","kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle","text":"ShapeStyle"},{"kind":"text","text":"\n"}],"platforms":["macOS"],"languages":["swift"]}],"kind":"declarations"},{"parameters":[{"content":[{"inlineContent":[{"type":"text","text":"The color or pattern to use when filling in the"},{"type":"text","text":" "},{"type":"text","text":"foreground elements. To indicate a specific value, use "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Color","isActive":true,"type":"reference"},{"type":"text","text":" or"},{"type":"text","text":" "},{"code":"ShapeStyle\/image(_:sourceRect:scale:)","type":"codeVoice"},{"type":"text","text":", or one of the gradient"},{"type":"text","text":" "},{"type":"text","text":"types, like"},{"type":"text","text":" "},{"code":"ShapeStyle\/linearGradient(colors:startPoint:endPoint:)","type":"codeVoice"},{"type":"text","text":". To set a"},{"type":"text","text":" "},{"type":"text","text":"style that’s relative to the containing view’s style, use one of the"},{"type":"text","text":" "},{"type":"text","text":"semantic styles, like "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/primary","isActive":true,"type":"reference"},{"type":"text","text":"."}],"type":"paragraph"}],"name":"style"}],"kind":"parameters"},{"content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"text":"A view that uses the given foreground style.","type":"text"}]}],"kind":"content"},{"content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"Use this method to style","type":"text"},{"text":" ","type":"text"},{"text":"foreground content like text, shapes, and template images","type":"text"},{"text":" ","type":"text"},{"text":"(including symbols):","type":"text"}]},{"type":"codeListing","code":["HStack {","    Image(systemName: \"triangle.fill\")","    Text(\"Hello, world!\")","    RoundedRectangle(cornerRadius: 5)","        .frame(width: 40, height: 20)","}",".foregroundStyle(.teal)"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"The example above creates a row of ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/teal","type":"reference","isActive":true},{"text":" foreground","type":"text"},{"text":" ","type":"text"},{"text":"elements:","type":"text"}]},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"type":"text","text":"You can use any style that conforms to the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle"},{"type":"text","text":" protocol,"},{"type":"text","text":" "},{"type":"text","text":"like the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/teal"},{"type":"text","text":" color in the example above, or the"},{"type":"text","text":" "},{"type":"codeVoice","code":"ShapeStyle\/linearGradient(colors:startPoint:endPoint:)"},{"type":"text","text":" gradient"},{"type":"text","text":" "},{"type":"text","text":"shown below:"}]},{"type":"codeListing","code":["Text(\"Gradient Text\")","    .font(.largeTitle)","    .foregroundStyle(","        .linearGradient(","            colors: [.yellow, .blue],","            startPoint: .top,","            endPoint: .bottom","        )","    )"],"syntax":"swift"},{"type":"paragraph","inlineContent":[]},{"content":[{"inlineContent":[{"type":"text","text":"If you want to fill a single "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Shape","isActive":true},{"type":"text","text":" instance with a style,"},{"type":"text","text":" "},{"type":"text","text":"use the "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Shape\/fill(style:)","isActive":true},{"type":"text","text":" shape modifier instead because it’s more"},{"type":"text","text":" "},{"type":"text","text":"efficient."}],"type":"paragraph"}],"type":"aside","name":"Tip","style":"tip"},{"type":"paragraph","inlineContent":[{"text":"OpenSwiftUI creates a context-dependent render for a given style.","type":"text"},{"text":" ","type":"text"},{"text":"For example, a ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Color"},{"text":" that you load from an asset catalog","type":"text"},{"text":" ","type":"text"},{"text":"can have different light and dark appearances, while some styles","type":"text"},{"text":" ","type":"text"},{"text":"also vary by platform.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Hierarchical foreground styles like "},{"type":"codeVoice","code":"ShapeStyle\/secondary"},{"type":"text","text":" "},{"type":"text","text":"don’t impose a style of their own, but instead modify other styles."},{"type":"text","text":" "},{"type":"text","text":"In particular, they modify the primary"},{"type":"text","text":" "},{"type":"text","text":"level of the current foreground style to the degree given by"},{"type":"text","text":" "},{"type":"text","text":"the hierarchical style’s name."},{"type":"text","text":" "},{"type":"text","text":"To find the current foreground style to modify, OpenSwiftUI looks for"},{"type":"text","text":" "},{"type":"text","text":"the innermost containing style that you apply with the"},{"type":"text","text":" "},{"type":"codeVoice","code":"foregroundStyle(_:)"},{"type":"text","text":" or the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/foregroundColor(_:)"},{"type":"text","text":" modifier."},{"type":"text","text":" "},{"type":"text","text":"If you haven’t specified a style, OpenSwiftUI uses the default foreground"},{"type":"text","text":" "},{"type":"text","text":"style, as in the following example:"}]},{"type":"codeListing","code":["VStack(alignment: .leading) {","    Label(\"Primary\", systemImage: \"1.square.fill\")","    Label(\"Secondary\", systemImage: \"2.square.fill\")","        .foregroundStyle(.secondary)","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If you add a foreground style on the enclosing"},{"type":"text","text":" "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack","type":"reference","isActive":true},{"type":"text","text":", the hierarchical styling responds accordingly:"}]},{"type":"codeListing","code":["VStack(alignment: .leading) {","    Label(\"Primary\", systemImage: \"1.square.fill\")","    Label(\"Secondary\", systemImage: \"2.square.fill\")","        .foregroundStyle(.secondary)","}",".foregroundStyle(.blue)"],"syntax":"swift"},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"text":"When you apply a custom style to a view, the view disables the vibrancy","type":"text"},{"text":" ","type":"text"},{"text":"effect for foreground elements in that view, or in any of its child","type":"text"},{"text":" ","type":"text"},{"text":"views, that it would otherwise gain from adding a background material","type":"text"},{"text":" ","type":"text"},{"text":"— for example, using the ","type":"text"},{"type":"codeVoice","code":"View\/background(_:ignoresSafeAreaEdges:)"},{"text":" ","type":"text"},{"text":"modifier. However, hierarchical styles applied to the default foreground","type":"text"},{"text":" ","type":"text"},{"text":"don’t disable vibrancy.","type":"text"}]}],"kind":"content"}],"sections":[],"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PrimitiveButtonStyleConfiguration","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PrimitiveButtonStyleConfiguration\/Label-swift.struct"]]},"kind":"symbol","metadata":{"role":"symbol","extendedModule":"OpenSwiftUICore","modules":[{"name":"OpenSwiftUI","relatedModules":["OpenSwiftUICore"]}],"platforms":[{"unavailable":false,"beta":false,"deprecated":false,"name":"iOS","introducedAt":"13.0"},{"unavailable":false,"beta":false,"deprecated":false,"name":"macOS","introducedAt":"10.15"},{"unavailable":false,"beta":false,"deprecated":false,"name":"tvOS","introducedAt":"13.0"},{"unavailable":false,"beta":false,"deprecated":false,"name":"visionOS","introducedAt":"1.0"},{"unavailable":false,"beta":false,"deprecated":false,"name":"watchOS","introducedAt":"6.0"}],"title":"foregroundStyle(_:)","roleHeading":"Instance Metho