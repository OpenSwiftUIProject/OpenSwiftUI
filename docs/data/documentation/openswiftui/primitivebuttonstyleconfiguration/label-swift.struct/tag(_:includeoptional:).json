{"schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PrimitiveButtonStyleConfiguration\/Label-swift.struct\/tag(_:includeOptional:)","interfaceLanguage":"swift"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"attribute","text":"nonisolated"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"tag"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"V"},{"kind":"text","text":">("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"tag"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"V"},{"kind":"text","text":", "},{"kind":"externalParam","text":"includeOptional"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"kind":"text","text":" = true) -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","text":"View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP"},{"kind":"text","text":" "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"V"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"Hashable","preciseIdentifier":"s:SH"},{"kind":"text","text":"\n"}],"platforms":["macOS"],"languages":["swift"]}]},{"kind":"parameters","parameters":[{"name":"tag","content":[{"inlineContent":[{"type":"text","text":"A "},{"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","isActive":true},{"type":"text","text":" "},{"type":"text","text":"value to use as the view’s tag."}],"type":"paragraph"}]},{"name":"includeOptional","content":[{"type":"paragraph","inlineContent":[{"text":"If the tag value for ","type":"text"},{"code":"Optional<V>","type":"codeVoice"},{"text":" should","type":"text"},{"text":" ","type":"text"},{"text":"also be set.","type":"text"}]}]}]},{"kind":"content","content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A view with the specified tag set."}]}]},{"kind":"content","content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"inlineContent":[{"text":"Use this modifier to differentiate among certain selectable views,","type":"text"},{"text":" ","type":"text"},{"text":"like the possible values of a ","type":"text"},{"type":"codeVoice","code":"Picker"},{"text":" or the tabs of a ","type":"text"},{"type":"codeVoice","code":"TabView"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"Tag values can be of any type that conforms to the","type":"text"},{"text":" ","type":"text"},{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","type":"reference","isActive":true},{"type":"text","text":" protocol."}],"type":"paragraph"},{"inlineContent":[{"text":"This modifier will write the tag value for the type ","type":"text"},{"type":"codeVoice","code":"V"},{"text":", as well as","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"Optional<V>"},{"text":" if ","type":"text"},{"type":"codeVoice","code":"includeOptional"},{"text":" is enabled. Containers checking for","type":"text"},{"text":" ","type":"text"},{"text":"tags of either type will see the value as set.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"In the example below, the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","type":"reference","isActive":true},{"text":" loop in the ","type":"text"},{"type":"codeVoice","code":"Picker"},{"text":" view","type":"text"},{"text":" ","type":"text"},{"text":"builder iterates over the ","type":"text"},{"type":"codeVoice","code":"Flavor"},{"text":" enumeration. It extracts the string","type":"text"},{"text":" ","type":"text"},{"text":"value of each enumeration element for use in constructing the row","type":"text"},{"text":" ","type":"text"},{"text":"label, and uses the enumeration value as input to the ","type":"text"},{"type":"codeVoice","code":"tag(_:)"},{"text":" ","type":"text"},{"text":"modifier.","type":"text"}],"type":"paragraph"},{"code":["struct FlavorPicker: View {","    enum Flavor: String, CaseIterable, Identifiable {","        case chocolate, vanilla, strawberry","        var id: Self { self }","    }","","    @State private var selectedFlavor: Flavor? = nil","","    var body: some View {","        Picker(\"Flavor\", selection: $selectedFlavor) {","            ForEach(Flavor.allCases) { flavor in","                Text(flavor.rawValue)","                    .tag(flavor)","            }","        }","    }","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"The selection type of the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":" is an "},{"type":"codeVoice","code":"Optional<Flavor>"},{"type":"text","text":" and so it"},{"type":"text","text":" "},{"type":"text","text":"will look for tags on its contents of "},{"type":"codeVoice","code":"Optional<Flavor>"},{"type":"text","text":"type. Since the"},{"type":"text","text":" "},{"type":"text","text":"tag modifier defaults to having "},{"type":"codeVoice","code":"includeOptional"},{"type":"text","text":" enabled, even though"},{"type":"text","text":" "},{"type":"text","text":"the tag for each option is a non-optional "},{"type":"codeVoice","code":"Flavor"},{"type":"text","text":", the tag modifier"},{"type":"text","text":" "},{"type":"text","text":"writes values for both the non-optional, and optional versions of the"},{"type":"text","text":" "},{"type":"text","text":"value, allowing the contents to be selectable by the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"text":"A ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","type":"reference","isActive":true},{"text":" automatically applies a default tag to each enumerated","type":"text"},{"text":" ","type":"text"},{"text":"view using the ","type":"text"},{"code":"id","type":"codeVoice"},{"text":" parameter of the corresponding element. If","type":"text"},{"text":" ","type":"text"},{"text":"the element’s ","type":"text"},{"code":"id","type":"codeVoice"},{"text":" parameter and the picker’s ","type":"text"},{"code":"selection","type":"codeVoice"},{"text":" input","type":"text"},{"text":" ","type":"text"},{"text":"have exactly the same type, or the same type but optional, you can omit","type":"text"},{"text":" ","type":"text"},{"text":"the explicit tag modifier.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"To see examples that don’t require an explicit tag, see ","type":"text"},{"type":"codeVoice","code":"Picker"},{"text":".","type":"text"}],"type":"paragraph"}]}],"variants":[{"paths":["\/documentation\/openswiftui\/primitivebuttonstyleconfiguration\/label-swift.struct\/tag(_:includeoptional:)"],"traits":[{"interfaceLanguage":"swift"}]}],"sections":[],"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PrimitiveButtonStyleConfiguration","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PrimitiveButtonStyleConfiguration\/Label-swift.struct"]]},"abstract":[{"type":"text","text":"Sets the unique tag value of this view."}],"metadata":{"modules":[{"name":"OpenSwiftUI","relatedModules":["OpenSwiftUICore"]}],"symbolKind":"method","role":"symbol","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"tag","kind":"identifier"},{"text":"<","kind":"text"},{"text":"V","kind":"genericParameter"},{"text":">(","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"includeOptional","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"Bool","preciseIdentifier":"s:Sb","kind":"t