 do {","                var receivedLines = [String]()","                for try await line in url.lines {","                    receivedLines.append(line)","                    message = \"Received \\(receivedLines.count) lines\"","                }","            } catch {","                message = \"Failed to load\"","            }","        }","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This example uses the"},{"type":"text","text":" "},{"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/url\/3767315-lines","isActive":true},{"type":"text","text":" "},{"type":"text","text":"method to get the content stored at the specified"},{"type":"text","text":" "},{"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/url","isActive":true},{"type":"text","text":" as an"},{"type":"text","text":" "},{"type":"text","text":"asynchronous sequence of strings. When each new line arrives, the body"},{"type":"text","text":" "},{"type":"text","text":"of the "},{"type":"codeVoice","code":"for"},{"type":"text","text":"-"},{"type":"codeVoice","code":"await"},{"type":"text","text":"-"},{"type":"codeVoice","code":"in"},{"type":"text","text":" loop stores the line in an array of strings"},{"type":"text","text":" "},{"type":"text","text":"and updates the content of the text view to report the latest line"},{"type":"text","text":" "},{"type":"text","text":"count."}]}],"kind":"content"}],"references":{"https://developer.apple.com/documentation/swift/asyncsequence":{"title":"AsyncSequence","titleInlineContent":[{"text":"AsyncSequence","type":"text"}],"url":"https:\/\/developer.apple.com\/documentation\/swift\/asyncsequence","type":"link","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/asyncsequence"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/Text":{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","fragments":[{"kind":"