ractivity/1418086-webpageurl":{"title":"webpageURL","type":"link","titleInlineContent":[{"type":"text","text":"webpageURL"}],"url":"https:\/\/developer.apple.com\/documentation\/foundation\/nsuseractivity\/1418086-webpageurl","identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/nsuseractivity\/1418086-webpageurl"}}}