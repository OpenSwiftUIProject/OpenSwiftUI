ifier"},{"text":"<","kind":"text"},{"text":"V","kind":"genericParameter"},{"text":">(","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"tag","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"includeOptional","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"},{"text":" = true) -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","kind":"typeIdentifier","preciseIdentifier":"s:15OpenSwiftUICore4ViewP","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"text":" ","kind":"text"},{"text":"where","kind":"keyword"},{"text":" ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"Hashable","kind":"typeIdentifier","preciseIdentifier":"s:SH"},{"text":"\n","kind":"text"}],"languages":["swift"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A "},{"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","isActive":true},{"type":"text","text":" "},{"type":"text","text":"value to use as the view’s tag."}]}],"name":"tag"},{"content":[{"inlineContent":[{"text":"If the tag value for ","type":"text"},{"code":"Optional<V>","type":"codeVoice"},{"text":" should","type":"text"},{"text":" ","type":"text"},{"text":"also be set.","type":"text"}],"type":"paragraph"}],"name":"includeOptional"}]},{"kind":"content","content":[{"anchor":"return-value","level":2,"type":"heading","text":"Return Value"},{"inlineContent":[{"type":"text","text":"A view with the specified tag set."}],"type":"paragraph"}]},{"kind":"content","content":[{"anchor":"discussion","type":"heading","level":2,"text":"Discussion"},{"inlineContent":[{"text":"Use this modifier to differentiate among certain selectable views,","type":"text"},{"text":" ","type":"text"},{"text":"like the possible values of a ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" or the tabs of a ","type":"text"},{"code":"TabView","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"Tag values can be of any type that conforms to the","type":"text"},{"text":" ","type":"text"},{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","isActive":true,"type":"reference"},{"text":" protocol.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"This modifier will write the tag value for the type ","type":"text"},{"code":"V","type":"codeVoice"},{"text":", as well as","type":"text"},{"text":" ","type":"text"},{"code":"Optional<V>","type":"codeVoice"},{"text":" if ","type":"text"},{"code":"includeOptional","type":"codeVoice"},{"text":" is enabled. Containers checking for","type":"text"},{"text":" ","type":"text"},{"text":"tags of either type will see the value as set.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"In the example below, the "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true,"type":"reference"},{"type":"text","text":" loop in the "},{"code":"Picker","type":"codeVoice"},{"type":"text","text":" view"},{"type":"text","text":" "},{"type":"text","text":"builder iterates over the "},{"code":"Flavor","type":"codeVoice"},{"type":"text","text":" enumeration. It extracts the string"},{"type":"text","text":" "},{"type":"text","text":"value of each enumeration element for use in constructing the row"},{"type":"text","text":" "},{"type":"text","text":"label, and uses the enumeration value as input to the "},{"code":"tag(_:)","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"modifier."}],"type":"paragraph"},{"type":"codeListing","code":["struct FlavorPicker: View {","    enum Flavor: String, CaseIterable, Identifiable {","        case chocolate, vanilla, strawberry","        var id: Self { self }","    }","","    @State private var selectedFlavor: Flavor? = nil","","    var body: some View {","        Picker(\"Flavor\", selection: $selectedFlavor) {","            ForEach(Flavor.allCases) { flavor in","                Text(flavor.rawValue)","                    .tag(flavor)","            }","        }","    }","}"],"syntax":"swift"},{"inlineContent":[{"text":"The selection type of the ","type":"text"},{"type":"codeVoice","code":"Picker"},{"text":" is an ","type":"text"},{"type":"codeVoice","code":"Optional<Flavor>"},{"text":" and so it","type":"text"},{"text":" ","type":"text"},{"text":"will look for tags on its contents of ","type":"text"},{"type":"codeVoice","code":"Optional<Flavor>"},{"text":"type. Since the","type":"text"},{"text":" ","type":"text"},{"text":"tag modifier defaults to having ","type":"text"},{"type":"codeVoice","code":"includeOptional"},{"type":"text","text":" enabled, even though"},{"type":"text","text":" "},{"type":"text","text":"the tag for each option is a non-optional "},{"type":"codeVoice","code":"Flavor"},{"type":"text","text":", the tag modifier"},{"type":"text","text":" "},{"type":"text","text":"writes values for both the non-optional, and optional versions of the"},{"type":"text","text":" "},{"type":"text","text":"value, allowing the contents to be selectable by the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"text":"A ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true,"type":"reference"},{"text":" automatically applies a default tag to each enumerated","type":"text"},{"text":" ","type":"text"},{"text":"view using the ","type":"text"},{"code":"id","type":"codeVoice"},{"text":" parameter of the corresponding element. If","type":"text"},{"text":" ","type":"text"},{"text":"the element’s ","type":"text"},{"code":"id","type":"codeVoice"},{"text":" parameter and the picker’s ","type":"text"},{"code":"selection","type":"codeVoice"},{"text":" input","type":"text"},{"text":" ","type":"text"},{"text":"have exactly the same type, or the same type but optional, you can omit","type":"text"},{"text":" ","type":"text"},{"text":"the explicit tag modifier.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"To see examples that don’t require an explicit tag, see "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":"."}],"type":"paragraph"}]}],"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyView"]]},"abstract":[{"text":"Sets the unique tag value of this view.","type":"text"}],"kind":"symbol","identifier":{"url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyView\/tag(_:includeOptional:)","interfaceLanguage":"swift"},"references":{"https://developer.apple.com/documentation/swift/hashable":{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","url":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","title":"Hashable","type":"link","titleInlineContent":[{"text":"Hashable","type":"text"}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/AnyView":{"abstract":[{"text":"A type-erased view.","type":"text"}],"kind":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AnyView","kind":"identifier"}],"type":"topic","title":"AnyView","navigatorTitle":[{"text":"AnyView","kind":"identifier"}],"url":"\/documentation\/openswiftui\/anyview","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyView","role":"symbol"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/View":{"title":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","type":"topic","kind":"symbol","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","kind":"identifier"}],"role":"symbol","url":"\/documentation\/openswiftui\/view","abstract":[{"text":"A type t