ations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"text":"nonisolated","kind":"attribute"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"task","kind":"identifier"},{"text":"(","kind":"text"},{"text":"priority","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"TaskPriority","preciseIdentifier":"s:ScP","kind":"typeIdentifier"},{"text":" = .userInitiated, ","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"action","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"@escaping","kind":"attribute"},{"text":" () ","kind":"text"},{"text":"async","kind":"keyword"},{"text":" -> ","kind":"text"},{"text":"Void","preciseIdentifier":"s:s4Voida","kind":"typeIdentifier"},{"text":") -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","kind":"typeIdentifier"},{"text":"\n","kind":"text"}]}]},{"kind":"parameters","parameters":[{"content":[{"inlineContent":[{"type":"text","text":"The task priority to use when creating the asynchronous"},{"type":"text","text":" "},{"type":"text","text":"task. The default priority is"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/taskpriority\/userinitiated"},{"type":"text","text":","}],"type":"paragraph"}],"name":"priority"},{"content":[{"inlineContent":[{"type":"text","text":"A closure that OpenSwiftUI calls as an asynchronous task"},{"type":"text","text":" "},{"type":"text","text":"before the view appears. OpenSwiftUI will automatically cancel the task"},{"type":"text","text":" "},{"type":"text","text":"at some point after the view disappears before the action completes."}],"type":"paragraph"}],"name":"action"}]},{"content":[{"text":"Return Value","level":2,"anchor":"return-value","type":"heading"},{"inlineContent":[{"type":"text","text":"A view that runs the specified action asynchronously before"},{"type":"text","text":" "},{"type":"text","text":"the view appears."}],"type":"paragraph"}],"kind":"content"},{"content":[{"anchor":"discussion","type":"heading","text":"Discussion","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"Use this modifier to perform an asynchronous task with a lifetime that"},{"type":"text","text":" "},{"type":"text","text":"matches that of the modified view. If the task doesn’t finish"},{"type":"text","text":" "},{"type":"text","text":"before OpenSwiftUI removes the view or the view changes identity, OpenSwiftUI"},{"type":"text","text":" "},{"type":"text","text":"cancels the task."}]},{"type":"paragraph","inlineContent":[{"text":"Use the ","type":"text"},{"code":"await","type":"codeVoice"},{"text":" keyword inside the task to","type":"text"},{"text":" ","type":"text"},{"text":"wait for an asynchronous call to complete, or to wait on the values of","type":"text"},{"text":" ","type":"text"},{"text":"an ","type":"text"},{"isActive":true,"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/asyncsequence","type":"reference"},{"text":" ","type":"text"},{"text":"instance. For example, you can modify a ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","type":"reference"},{"text":" view to start a task","type":"text"},{"text":" ","type":"text"},{"text":"that loads content from a remote resource:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["let url = URL(string: \"https:\/\/example.com\")!","@State private var message = \"Loading...\"","","var body: some View {","    Text(message)","        .task {","            do {","                var receivedLines = [String]()","                for try await line in url.lines {","                    receivedLines.append(line)","                    message = \"Received \\(receivedLines.count) lines\"","                }","            } catch {","                message = \"Failed to load\"","            }","        }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"This example uses the"},{"type":"text","text":" "},{"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/url\/3767315-lines","isActive":true},{"type":"text","text":" "},{"type":"text","text":"method to get the content stored at the specified"},{"type":"text","text":" "},{"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/url","isActive":true},{"type":"text","text":" as an"},{"type":"text","text":" "},{"type":"text","text":"asynchronous sequence of strings. When each new line arrives, the body"},{"type":"text","text":" "},{"type":"text","text":"of the "},{"type":"codeVoice","code":"for"},{"type":"text","text":"-"},{"type":"codeVoice","code":"await"},{"type":"text","text":"-"},{"type":"codeVoice","code":"in"},{"type":"text","text":" loop stores the line in an array of strings"},{"type":"text","text":" "},{"type":"text","text":"and updates the content of the text view to report the latest line"},{"type":"text","text":" "},{"type":"text","text":"count."}]}],"kind":"content"}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LabeledContent\/task(priority:_:)"},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/openswiftui\/labeledcontent\/task(priority:_:)"]}],"sections":[],"references":{"https://developer.apple.com/documentation/swift/taskpriority/userinitiated":{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/taskpriority\/userinitiated","type":"link","title":"userInitiated","url":"https:\/\/developer.apple.com\/documentation\/swift\/taskpriority\/userinitiated","titleInlineContent":[{"type":"text","text":"userInitiated"}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/LabeledContent":{"type":"topic","url":"\