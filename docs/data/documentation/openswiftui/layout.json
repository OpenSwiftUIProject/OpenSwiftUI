tems":[{"content":[{"inlineContent":[{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/sizeThatFits(proposal:subviews:cache:)","isActive":true},{"text":" ","type":"text"},{"text":"reports the size of the composite layout view.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/placeSubviews(in:proposal:subviews:cache:)"},{"type":"text","text":" "},{"type":"text","text":"assigns positions to the container’s subviews."}]}]}],"type":"unorderedList"},{"inlineContent":[{"text":"You can define a basic layout type with only these two methods:","type":"text"}],"type":"paragraph"},{"code":["struct BasicVStack: Layout {","    func sizeThatFits(","        proposal: ProposedViewSize,","        subviews: Subviews,","        cache: inout ()","    ) -> CGSize {","        \/\/ Calculate and return the size of the layout container.","    }","","    func placeSubviews(","        in bounds: CGRect,","        proposal: ProposedViewSize,","        subviews: Subviews,","        cache: inout ()","    ) {","        \/\/ Tell each subview where to appear.","    }","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"text":"Use your layout the same way you use a built-in layout","type":"text"},{"text":" ","type":"text"},{"text":"container, by providing a ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ViewBuilder","isActive":true,"type":"reference"},{"text":" with the list of subviews","type":"text"},{"text":" ","type":"text"},{"text":"to arrange:","type":"text"}],"type":"paragraph"},{"code":["BasicVStack {","    Text(\"A Subview\")","    Text(\"Another Subview\")","}"],"type":"codeListing","syntax":"swift"},{"anchor":"Support-additional-behaviors","level":3,"type":"heading","text":"Support additional behaviors"},{"inlineContent":[{"text":"You can optionally implement other protocol methods and properties to","type":"text"},{"text":" ","type":"text"},{"text":"provide more layout container features:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Define explicit horizontal and vertical layout guides for the container by"},{"type":"text","text":" "},{"type":"text","text":"implementing "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/explicitAlignment(of:in:proposal:subviews:cache:)","isActive":true,"type":"reference"},{"type":"text","text":" "},{"type":"text","text":"for each dimension."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Establish the preferred spacing around the container by implementing"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/spacing(subviews:cache:)"},{"type":"text","text":"."}]}]},{"content":[{"inlineContent":[{"text":"Indicate the axis of orientation for a container that has characteristics","type":"text"},{"text":" ","type":"text"},{"text":"of a stack by implementing the ","type":"text"},{"type":"codeVoice","code":"layoutProperties-5rb5b"},{"text":" static property.","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Create and manage a cache to store computed values across different"},{"type":"text","text":" "},{"type":"text","text":"layout protocol calls by implementing "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/makeCache(subviews:)"},{"type":"text","text":"."}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"The protocol provides default implementations for these symbols"},{"type":"text","text":" "},{"type":"text","text":"if you don’t implement them. See each method or property for details."}],"type":"paragraph"},{"anchor":"Add-input-parameters","level":3,"type":"heading","text":"Add input parameters"},{"inlineContent":[{"type":"text","text":"You can define parameters as inputs to the layout, like you might"},{"type":"text","text":" "},{"type":"text","text":"for a "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"type":"text","text":":"}],"type":"paragraph"},{"code":["struct BasicVStack: Layout {","    var alignment: HorizontalAlignment","","    \/\/ ...","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"Set the parameters at the point where you instantiate the layout:"}],"type":"paragraph"},{"code":["BasicVStack(alignment: .leading) {","    \/\/ ...","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"If the layout provides default values for its parameters, you can omit the"},{"type":"text","text":" "},{"type":"text","text":"parameters at the call site, but you might need to keep the parentheses"},{"type":"text","text":" "},{"type":"text","text":"after the name of the layout, depending on how you specify the defaults."},{"type":"text","text":" "},{"type":"text","text":"For example, suppose you set a default alignment for the basic stack in"},{"type":"text","text":" "},{"type":"text","text":"the parameter declaration:"}],"type":"paragraph"},{"code":["struct BasicVStack: Layout {","    var alignment: HorizontalAlignment = .center","","    \/\/ ...","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"text":"To instantiate this layout using the default center alignment, you don’t","type":"text"},{"text":" ","type":"text"},{"text":"have to specify the alignment value, but you do need to add empty","type":"text"},{"text":" ","type":"text"},{"text":"parentheses:","type":"text"}],"type":"paragraph"},{"code":["BasicVStack() {","    \/\/ ...","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"The Swift compiler requires the parentheses in this case because of how the"},{"type":"text","text":" "},{"type":"text","text":"layout protocol implements this call site syntax. Specifically, the layout’s"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/callAsFunction(_:)","isActive":true},{"type":"text","text":" method looks for an initializer with exactly zero"},{"type":"text","text":" "},{"type":"text","text":"input arguments when you omit the parentheses from the call site."},{"type":"text","text":" "},{"type":"text","text":"You can enable the simpler call site for a layout that doesn’t have an"},{"type":"text","text":" "},{"type":"text","text":"implicit initializer of this type by explicitly defining one:"}],"type":"paragraph"},{"code":["init() {","    self.alignment = .center","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"For information about Swift initializers, see"},{"type":"text","text":" "},{"identifier":"https:\/\/docs.swift.org\/swift-book\/LanguageGuide\/Initialization.html","isActive":true,"type":"reference"},{"type":"text","text":" "},{"type":"text","text":"in "},{"type":"emphasis","inlineContent":[{"text":"The Swift Programming Language","type":"text"}]},{"type":"text","text":"."}],"type":"paragraph"},{"anchor":"Interact-with-subviews-through-their-proxies","level":3,"type":"heading","text":"Interact with subviews through their proxies"},{"inlineContent":[{"type":"text","text":"To perform layout, you need information about all of its subviews, which"},{"type":"text","text":" "},{"type":"text","text":"are the views that your container arranges. While your layout can’t"},{"type":"text","text":" "},{"type":"text","text":"interact directly with its subviews, it can access a set of subview proxies"},{"type":"text","text":" "},{"type":"text","text":"through the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/Subviews"},{"type":"text","text":" collection that each protocol method receives as"},{"type":"text","text":" "},{"type":"text","text":"an input parameter. That type is an alias for the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutSubviews"},{"type":"text","text":" "},{"type":"text","text":"collection type, which in turn contains "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutSubview"},{"type":"text","text":" instances"},{"type":"text","text":" "},{"type":"text","text":"that are the subview proxies."}],"type":"paragraph"},{"inlineContent":[{"text":"You can get information about each subview from its proxy, like its","type":"text"},{"text":" ","type":"text"},{"text":"dimensions and spacing preferences. This enables","type":"text"},{"text":" ","type":"text"},{"text":"you to measure subviews before you commit to placing them. You also","type":"text"},{"text":" ","type":"text"},{"text":"assign a position to each subview by calling its proxy’s","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutSubview\/place(at:anchor:proposal:)","type":"reference"},{"text":" method.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Call the method on each subview from within your implementation of the"},{"type":"text","text":" "},{"type":"text","text":"layout’s "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/placeSubviews(in:proposal:subviews:cache:)"},{"type":"text","text":" method."}],"type":"paragraph"},{"anchor":"Access-layout-values","level":3,"type":"heading","text":"Access layout values"},{"inlineContent":[{"text":"Views have layout values that you set with view modifiers.","type":"text"},{"text":" ","type":"text"},{"text":"Layout containers can choose to condition their behavior accordingly.","type":"text"},{"text":" ","type":"text"},{"text":"For example, a built-in ","type":"text"},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/HStack","isActive":true},{"text":" allocates space to its subviews based","type":"text"},{"text":" ","type":"text"},{"text":"in part on the priorities that you set with the ","type":"text"},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/layoutPriority(_:)","isActive":true},{"text":" ","type":"text"},{"text":"view modifier. Your layout container accesses this value for a subview by","type":"text"},{"text":" ","type":"text"},{"text":"reading the proxy’s ","type":"text"},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutSubview\/priority","isActive":true},{"text":" property.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"You can also create custom layout values by creating a layout key.","type":"text"},{"text":" ","type":"text"},{"text":"Set a value on a view with the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/layoutValue(key:value:)","isActive":true,"type":"reference"},{"text":" view","type":"text"},{"text":" ","type":"text"},{"text":"modifier. Read the corresponding value from the subview’s proxy using the","type":"text"},{"text":" ","type":"text"},{"text":"key as an index on the subview. For more information about creating,","type":"text"},{"text":" ","type":"text"},{"text":"setting, and accessing custom layout values, see ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutValueKey","isActive":true,"type":"reference"},{"text":".","type":"text"}],"type":"paragraph"}]}],"relationshipsSections":[{"kind":"relationships","type":"inheritsFrom","identifiers":["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Animatable"],"title":"Inherits From"},{"kind":"relationships","type":"conformingTypes","identifiers":["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyLay