ns":[{"kind":"attribute","text":"nonisolated"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"multilineTextAlignment"},{"kind":"text","text":"("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"alignment"},{"kind":"text","text":": "},{"preciseIdentifier":"s:15OpenSwiftUICore13TextAlignmentO","kind":"typeIdentifier","text":"TextAlignment","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/TextAlignment"},{"kind":"text","text":") -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"preciseIdentifier":"s:15OpenSwiftUICore4ViewP","kind":"typeIdentifier","text":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"kind":"text","text":"\n"}],"platforms":["macOS"]}],"kind":"declarations"},{"parameters":[{"name":"alignment","content":[{"inlineContent":[{"type":"text","text":"A value that you use to align multiple lines of"},{"type":"text","text":" "},{"type":"text","text":"text within a view."}],"type":"paragraph"}]}],"kind":"parameters"},{"content":[{"type":"heading","level":2,"text":"Return Value","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"text":"A view that aligns the lines of multiline ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","type":"reference","isActive":true},{"text":" instances","type":"text"},{"text":" ","type":"text"},{"text":"it contains.","type":"text"}]}],"kind":"content"},{"content":[{"text":"Discussion","type":"heading","anchor":"discussion","level":2},{"inlineContent":[{"type":"text","text":"Use this modifier to set an alignment for a multiline block of text."},{"type":"text","text":" "},{"type":"text","text":"For example, the modifier centers the contents of the following"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","isActive":true},{"type":"text","text":" view:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["Text(\"This is a block of text that shows up in a text element as multiple lines.\\(\"\\n\") Here we have chosen to center this text.\")","    .frame(width: 200)","    .multilineTextAlignment(.center)"]},{"inlineContent":[{"type":"text","text":"The text in the above example spans more than one line because:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The newline character introduces a line break.","type":"text"}]}]},{"content":[{"inlineContent":[{"text":"The frame modifier limits the space available to the text view, and","type":"text"},{"text":" ","type":"text"},{"text":"by default a text view wraps lines that don’t fit in the available","type":"text"},{"text":" ","type":"text"},{"text":"width. As a result, the text before the explicit line break wraps to","type":"text"},{"text":" ","type":"text"},{"text":"three lines, and the text after uses two lines.","type":"text"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"The modifier applies the alignment to the all the lines of text in"},{"type":"text","text":" "},{"type":"text","text":"the view, regardless of why wrapping occurs:"}],"type":"paragraph"},{"inlineContent":[],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The modifier has no effect on a "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","isActive":true},{"type":"text","text":" view that contains only one"},{"type":"text","text":" "},{"type":"text","text":"line of text, because a text view has a width that exactly matches the"},{"type":"text","text":" "},{"type":"text","text":"width of its widest line. If you want to align an entire text view"},{"type":"text","text":" "},{"type":"text","text":"rather than its contents, set the aligment of its container, like a"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack","isActive":true},{"type":"text","text":" or a frame that you create with the"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/frame(minWidth:idealWidth:maxWidth:minHeight:idealHeight:maxHeight:alignment:)","isActive":true},{"type":"text","text":" "},{"type":"text","text":"modifier."}],"type":"paragraph"},{"content":[{"type":"paragraph","inlineContent":[{"text":"You can use this modifier to control the alignment of a ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","isActive":true,"type":"reference"},{"text":" ","type":"text"},{"text":"view that you create with the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text\/init(_:style:)","isActive":true,"type":"reference"},{"text":" initializer","type":"text"},{"text":" ","type":"text"},{"text":"to display localized dates and times, including when the view uses","type":"text"},{"text":" ","type":"text"},{"text":"only a single line, but only when that view appears in a widget.","type":"text"}]}],"style":"note","name":"Note","type":"aside"},{"inlineContent":[{"type":"text","text":"The modifier also affects the content alignment of other text container"},{"type":"text","text":" "},{"type":"text","text":"types, like "},{"type":"codeVoice","code":"TextEditor"},{"type":"text","text":" and "},{"type":"codeVoice","code":"TextField"},{"type":"text","text":". In those cases, the"},{"type":"text","text":" "},{"type":"text","text":"modifier sets the alignment even when the view contains only a single"},{"type":"text","text":" "},{"type":"text","text":"line because view’s width isn’t dictated by the width of the text it"},{"type":"text","text":" "},{"type":"text","text":"contains."}],"type":"paragraph"},{"inlineContent":[{"text":"The modifier operates by setting the","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftU