larations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"placeSubviews","kind":"identifier"},{"text":"(","kind":"text"},{"text":"in","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"bounds","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"CGRect","preciseIdentifier":"c:@S@CGRect","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"proposal","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"ProposedViewSize","preciseIdentifier":"s:15OpenSwiftUICore16ProposedViewSizeV","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ProposedViewSize","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"subviews","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"AnyLayout","preciseIdentifier":"s:15OpenSwiftUICore9AnyLayoutV","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyLayout","kind":"typeIdentifier"},{"text":".","kind":"text"},{"text":"Subviews","preciseIdentifier":"s:15OpenSwiftUICore6LayoutP8Subviewsa","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/Subviews","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"cache","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"inout","kind":"keyword"},{"text":" ","kind":"text"},{"text":"AnyLayout","preciseIdentifier":"s:15OpenSwiftUICore9AnyLayoutV","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyLayout","kind":"typeIdentifier"},{"text":".","kind":"text"},{"text":"Cache","preciseIdentifier":"s:15OpenSwiftUICore9AnyLayoutV5CacheV","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyLayout\/Cache","kind":"typeIdentifier"},{"text":")","kind":"text"}]}],"kind":"declarations"},{"parameters":[{"name":"bounds","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The region that the container view’s parent allocates to the"},{"type":"text","text":" "},{"type":"text","text":"container view, specified in the parent’s coordinate space."},{"type":"text","text":" "},{"type":"text","text":"Place all the container’s subviews within the region."},{"type":"text","text":" "},{"type":"text","text":"The size of this region matches a size that your container"},{"type":"text","text":" "},{"type":"text","text":"previously returned from a call to the"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyLayout\/sizeThatFits(proposal:subviews:cache:)","isActive":true},{"type":"text","text":" method."}]}]},{"name":"proposal","content":[{"type":"paragraph","inlineContent":[{"text":"The size proposal from which the container generated the","type":"text"},{"text":" ","type":"text"},{"text":"size that the parent used to create the ","type":"text"},{"code":"bounds","type":"codeVoice"},{"text":" parameter.","type":"text"},{"text":" ","type":"text"},{"text":"The parent might propose more than one size before calling the","type":"text"},{"text":" ","type":"text"},{"text":"placement method, but it always uses one of the proposals and the","type":"text"},{"text":" ","type":"text"},{"text":"corresponding returned size when placing the container.","type":"text"}]}]},{"name":"subviews","content":[{"inlineContent":[{"text":"A collection of proxies that represent the","type":"text"},{"text":" ","type":"text"},{"text":"views that the container arranges. Use the proxies in the collection","type":"text"},{"text":" ","type":"text"},{"text":"to get information about the subviews and to tell the subviews","type":"text"},{"text":" ","type":"text"},{"text":"where to appear.","type":"text"}],"type":"paragraph"}]},{"name":"cache","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Optional storage for calculated data that you can share among"},{"type":"text","text":" "},{"type":"text","text":"the methods of your custom layout container. See"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyLayout\/makeCache(subviews:)","isActive":true},{"type":"text","text":" for details."}]}]}],"kind":"parameters"},{"kind":"content","content":[{"anchor":"discussion","type":"heading","level":2,"text":"Discussion"},{"inlineContent":[{"type":"text","text":"OpenSwiftUI calls your implementation of this method to tell your"},{"type":"text","text":" "},{"type":"text","text":"custom layout container to place its subviews. From this method, call"},{"type":"text","text":" "},{"type":"text","text":"the "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutSubview\/place(at:anchor:proposal:)","type":"reference","isActive":true},{"type":"text","text":" method on each"},{"type":"text","text":" "},{"type":"text","text":"element in "},{"code":"subviews","type":"codeVoice"},{"type":"text","text":" to tell the subviews where to appear in the"},{"type":"text","text":" "},{"type":"text","text":"user interface."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"For example, you can create a basic vertical stack that places views"},{"type":"text","text":" "},{"type":"text","text":"in a column, with views horizontally aligned on their leading edge:"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["struct BasicVStack: Layout {","    func placeSubviews(","        in bounds: CGRect,","        proposal: ProposedViewSize,","        subviews: Subviews,","        cache: inout ()","    ) {","        var point = bounds.origin","        for subview in subviews {","            subview.place(at: point, anchor: .topLeading, proposal: .unspecified)","            point.y += subview.dimensions(in: .unspecified).height","        }","    }","","    \/\/ This layout also needs a sizeThatFits() implementation.","}"]},{"inlineContent":[{"text":"The example creates a placement point that starts at the origin of the","type":"text"},{"text":" ","type":"text"},{"text":"specified ","type":"text"},{"code":"bounds","type":"codeVoice"},{"text":" input and uses that to place the first subview. It","type":"text"},{"text":" ","type":"text"},{"text":"then moves the point in the y dimension by the subview’s height,","type":"text"},{"text":" ","type":"text"},{"text":"which it reads using the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutSubview\/dimensions(in:)","type":"reference"},{"text":" method.","type":"text"},{"text":" ","type":"text"},{"text":"This prepares the point for the next iteration of the loop. All","type":"text"},{"text":" ","type":"text"},{"text":"subview operations use an ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ProposedViewSize\/unspecified","type":"reference"},{"text":" size","type":"text"},{"text":" ","type":"text"},{"text":"proposal to indicate that subviews should use and report their ideal","type":"text"},{"text":" ","type":"text"},{"text":"size.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"A more complex layout container might add space between subviews","type":"text"},{"text":" ","type":"text"},{"text":"according to their ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutSubview\/spacing","isActive":true,"type":"reference"},{"text":" preferences, or a","type":"text"},{"text":" ","type":"text"},{"text":"fixed space based on input configuration. For example, you can extend","type":"text"},{"text":" ","type":"text"},{"text":"the basic vertical stack’s placement method to calculate the","type":"text"},{"text":" ","type":"text"},{"text":"preferred distances between adjacent subviews and store the results in","type":"text"},{"text":" ","type":"text"},{"text":"an array:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["let spacing: [CGFloat] = subviews.indices.dropLast().map { index in","    subviews[index].spacing.distance(","        to: subviews[index + 1].spacing,","        along: .vertical)","}"]},{"inlineContent":[{"type":"text","text":"The spacing’s "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ViewSpacing\/distance(to:along:)","isActive":true},{"type":"text","text":" method considers the"},{"type":"text","text":" "},{"type":"text","text":"preferences of adjacent views on the edge where they meet. It returns"},{"type":"text","text":" "},{"type":"text","text":"the smallest distance that satisfies both views’ preferences for the"},{"type":"text","text":" "},{"type":"text","text":"given edge. For example, if one view prefers at least "},{"type":"codeVoice","code":"2"},{"type":"text","text":" points on its"},{"type":"text","text":" "},{"type":"text","text":"bottom edge, and the next view prefers at least "},{"type":"codeVoice","code":"8"},{"type":"text","text":" points on its top"},{"type":"text","text":" "},{"type":"text","text":"edge, the distance method returns "},{"type":"codeVoice","code":"8"},{"type":"text","text":", because that’s the smallest"},{"type":"text","text":" "},{"type":"text","text":"value that satisfies both preferences."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Update the placement calculations to use the spacing values:"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["var point = bounds.origin","for (index, subview) in subviews.enumerated() {","    if index > 0 { point.y += spacing[index - 1] } \/\/ Add spacing.","    subview.place(at: point, anchor: .topLeading, proposal: .unspecified)","    point.y += subview.dimensions(in: .unspecified).height","}"]},{"inlineContent":[{"type":"text","text":"Be sure that you use computations during placement that are consistent"},{"type":"text","text":" "},{"type":"text","text":"with those in your implementation of other protocol methods for a given"},{"type":"text","text":" "},{"type":"text","text":"set of inputs. For example, if you add spacing during placement,"},{"type":"text","text":" "},{"type":"text","text":"make sure your implementation of"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyLayout\/sizeThatFits(proposal:subviews:cache:)","isActive":true},{"type":"text","text":" accounts for the extra space."},{"type":"text","text":" "},{"type":"text","text":"Similarly, if the sizing method returns different values for different"},{"type":"text","text":" "},{"type":"text","text":"size proposals, make sure the placement method responds to its"},{"type":"text","text":" "},{"type":"codeVoice","code":"proposal"},{"type":"text","text":" input in the same way."}],"type":"paragraph"}]}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/openswiftui\/anylayout\/placesubviews(in:proposal:subviews:cache:)"]}],"kind":"symbol","sections":[],"schemaVersion":{"minor":3,"patch":0,"major":0},"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/AnyLayout"]]},"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/LayoutSubview/place(at:anchor:proposal:)":{"type":"topic","url":"\/documentation\/openswiftui\/layoutsubview\/place(at:anchor:proposal:)","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LayoutSubview\/place(at:anchor:proposal:)","kind":"symbol","role":"symbol","title":"place(at:anchor:proposal:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"place"},{"kind":"text","text":"("},{"kind":"externalParam","text":"at"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"c:@S@CGPoint","text":"CGPoint"},{"kind":"text","text":", "},{"kind":"externalParam","text":"anchor"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:15OpenSwiftUICore9UnitPointV","text":"UnitPoint"},{"kind":"text","text":", "},{"kind":"externalParam","text":"proposal"},{"kind":"text","text":": "},{"kind":"typ