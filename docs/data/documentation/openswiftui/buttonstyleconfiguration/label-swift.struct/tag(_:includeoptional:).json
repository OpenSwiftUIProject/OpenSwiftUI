{"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ButtonStyleConfiguration","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ButtonStyleConfiguration\/Label-swift.struct"]]},"kind":"symbol","primaryContentSections":[{"declarations":[{"tokens":[{"kind":"attribute","text":"nonisolated"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"tag"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"V"},{"kind":"text","text":">("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"tag"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"V"},{"kind":"text","text":", "},{"kind":"externalParam","text":"includeOptional"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"},{"kind":"text","text":" = true) -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","kind":"typeIdentifier","preciseIdentifier":"s:15OpenSwiftUICore4ViewP","text":"View"},{"kind":"text","text":" "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"V"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","preciseIdentifier":"s:SH","text":"Hashable"},{"kind":"text","text":"\n"}],"languages":["swift"],"platforms":["macOS"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"content":[{"inlineContent":[{"type":"text","text":"A "},{"isActive":true,"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable"},{"type":"text","text":" "},{"type":"text","text":"value to use as the view’s tag."}],"type":"paragraph"}],"name":"tag"},{"content":[{"inlineContent":[{"text":"If the tag value for ","type":"text"},{"type":"codeVoice","code":"Optional<V>"},{"text":" should","type":"text"},{"text":" ","type":"text"},{"text":"also be set.","type":"text"}],"type":"paragraph"}],"name":"includeOptional"}]},{"kind":"content","content":[{"anchor":"return-value","type":"heading","level":2,"text":"Return Value"},{"type":"paragraph","inlineContent":[{"text":"A view with the specified tag set.","type":"text"}]}]},{"kind":"content","content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"inlineContent":[{"text":"Use this modifier to differentiate among certain selectable views,","type":"text"},{"text":" ","type":"text"},{"text":"like the possible values of a ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" or the tabs of a ","type":"text"},{"code":"TabView","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"Tag values can be of any type that conforms to the","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","isActive":true},{"text":" protocol.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"This modifier will write the tag value for the type ","type":"text"},{"type":"codeVoice","code":"V"},{"text":", as well as","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"Optional<V>"},{"text":" if ","type":"text"},{"type":"codeVoice","code":"includeOptional"},{"text":" is enabled. Containers checking for","type":"text"},{"text":" ","type":"text"},{"text":"tags of either type will see the value as set.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"In the example below, the "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","type":"reference","isActive":true},{"type":"text","text":" loop in the "},{"code":"Picker","type":"codeVoice"},{"type":"text","text":" view"},{"type":"text","text":" "},{"type":"text","text":"builder iterates over the "},{"code":"Flavor","type":"codeVoice"},{"type":"text","text":" enumeration. It extracts the string"},{"type":"text","text":" "},{"type":"text","text":"value of each enumeration element for use in constructing the row"},{"type":"text","text":" "},{"type":"text","text":"label, and uses the enumeration value as input to the "},{"code":"tag(_:)","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"modifier."}],"type":"paragraph"},{"code":["struct FlavorPicker: View {","    enum Flavor: String, CaseIterable, Identifiable {","        case chocolate, vanilla, strawberry","        var id: Self { self }","    }","","    @State private var selectedFlavor: Flavor? = nil","","    var body: some View {","        Picker(\"Flavor\", selection: $selectedFlavor) {","            ForEach(Flavor.allCases) { flavor in","                Text(flavor.rawValue)","                    .tag(flavor)","            }","        }","    }","}"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"text":"The selection type of the ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" is an ","type":"text"},{"code":"Optional<Flavor>","type":"codeVoice"},{"text":" and so it","type":"text"},{"text":" ","type":"text"},{"text":"will look for tags on its contents of ","type":"text"},{"code":"Optional<Flavor>","type":"codeVoice"},{"text":"type. Since the","type":"text"},{"text":" ","type":"text"},{"text":"tag modifier defaults to having ","type":"text"},{"code":"includeOptional","type":"codeVoice"},{"text":" enabled, even though","type":"text"},{"text":" ","type":"text"},{"text":"the tag for each option is a non-optional ","type":"text"},{"code":"Flavor","type":"codeVoice"},{"text":", the tag modifier","type":"text"},{"text":" ","type":"text"},{"text":"writes values for both the non-optional, and optional versions of the","type":"text"},{"text":" ","type":"text"},{"text":"value, allowing the contents to be selectable by the ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"A "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach"},{"type":"text","text":" automatically applies a default tag to each enumerated"},{"type":"text","text":" "},{"type":"text","text":"view using the "},{"code":"id","type":"codeVoice"},{"type":"text","text":" parameter of the corresponding element. If"},{"type":"text","text":" "},{"type":"text","text":"the element’s "},{"code":"id","type":"codeVoice"},{"type":"text","text":" parameter and the picker’s "},{"code":"selection","type":"codeVoice"},{"type":"text","text":" input"},{"type":"text","text":" "},{"type":"text","text":"have exactly the same type, or the same type but optional, you can omit"},{"type":"text","text":" "},{"type":"text","text":"the explicit tag modifier."}],"type":"paragraph"},{"inlineContent":[{"text":"To see examples that don’t require an explicit tag, see ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"}]}],"sections":[],"schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"title":"tag(_:includeOptional:)","platforms":[{"deprecated":false,"beta":false,"name":"iOS","introducedAt":"13.0","unavailable":false},{"deprecated":false,"beta":false,"name":"macOS","introducedAt":"10.15","unavailable":false},{"deprecated":false,"beta":false,"name":"tvOS","introducedAt":"13.0","unavailable":false},{"deprecated":false,"beta":false,"name":"visionOS","introducedAt":"1.0","unavailable":false},{"deprecated":false,"beta":false,"name":"watchOS","introducedAt":"6.0","unavailable":false}],"symbolKind":"method","remoteSource":{"url":"https:\/\/github.com\/OpenSwiftUIProject\/OpenSwiftUI\/blob\/main\/Sources\/OpenSwiftUICore\/View\/Tag.swift#L71","fileName":"Tag.swift"},"role":"symbol","externalID":"s:15OpenSwiftUICore4ViewPAAE3tag_15includeOptionalQrqd___SbtSHRd__lF::SYNTHESIZED::s:11OpenSwiftUI24ButtonStyleConfigurationV5LabelV","modules":[{"name":"OpenSwiftUI","relatedModules":["OpenSwiftUICore"]}],"fragments":[{"text