ions":[{"languages":["swift"],"tokens":[{"kind":"attribute","text":"nonisolated"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"padding"},{"kind":"text","text":"("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"edges"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Edge","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Edge","preciseIdentifier":"s:15OpenSwiftUICore4EdgeO"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"Set","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Edge\/Set","preciseIdentifier":"s:15OpenSwiftUICore4EdgeO3SetV"},{"kind":"text","text":" = .all, "},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"length"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"CGFloat","preciseIdentifier":"s:14CoreFoundation7CGFloatV"},{"kind":"text","text":"? = nil) -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP"},{"kind":"text","text":"\n"}],"platforms":["macOS"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"edges","content":[{"type":"paragraph","inlineContent":[{"text":"The set of edges to pad for this view. The default","type":"text"},{"text":" ","type":"text"},{"text":"is ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Edge\/Set\/all","isActive":true,"type":"reference"},{"text":".","type":"text"}]}]},{"name":"length","content":[{"inlineContent":[{"type":"text","text":"An amount, given in points, to pad this view on the"},{"type":"text","text":" "},{"type":"text","text":"specified edges. If you set the value to "},{"type":"codeVoice","code":"nil"},{"type":"text","text":", OpenSwiftUI uses"},{"type":"text","text":" "},{"type":"text","text":"a platform-specific default amount. The default value of this"},{"type":"text","text":" "},{"type":"text","text":"parameter is "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"."}],"type":"paragraph"}]}]},{"kind":"content","content":[{"anchor":"return-value","type":"heading","text":"Return Value","level":2},{"inlineContent":[{"type":"text","text":"A view that’s padded by the specified amount on the"},{"type":"text","text":" "},{"type":"text","text":"specified edges."}],"type":"paragraph"}]},{"kind":"content","content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"inlineContent":[{"text":"Use this modifier to add a specified amount of padding to one or more","type":"text"},{"text":" ","type":"text"},{"text":"edges of the view. Indicate the edges to pad by naming either a single","type":"text"},{"text":" ","type":"text"},{"text":"value from ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Edge\/Set","type":"reference","isActive":true},{"text":", or by specifying an","type":"text"},{"text":" ","type":"text"},{"identifier":"https:\/\/developer.apple.com\/documentation\/Swift\/OptionSet","type":"reference","isActive":true},{"text":" ","type":"text"},{"text":"that contains edge values:","type":"text"}],"type":"paragraph"},{"code":["VStack {","    Text(\"Text padded by 20 points on the bottom and trailing edges.\")","        .padding([.bottom, .trailing], 20)","        .border(.gray)","    Text(\"Unpadded text for comparison.\")","        .border(.yellow)","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"The order in which you apply modifiers matters. The example above"},{"type":"text","text":" "},{"type":"text","text":"applies the padding before applying the border to ensure that the"},{"type":"text","text":" "},{"type":"text","text":"border encompasses the padded region:"}],"type":"paragraph"},{"inlineContent":[],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"You can omit either or both of the parameters. If you omit the "},{"type":"codeVoice","code":"length"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"OpenSwiftUI uses a default amount of padding. If you"},{"type":"text","text":" "},{"type":"text","text":"omit the "},{"type":"codeVoice","code":"edges"},{"type":"text","text":", OpenSwiftUI applies the padding to all edges. Omit both"},{"type":"text","text":" "},{"type":"text","text":"to add a default padding all the way around a view. OpenSwiftUI chooses a"},{"type":"text","text":" "},{"type":"text","text":"default amount of padding that’s appropriate for the platform and"},{"type":"text","text":" "},{"type":"text","text":"the presentation context."}],"type":"paragraph"},{"code":["VStack {","    Text(\"Text with default padding.\")","        .padding()","        .border(.gray)","    Text(\"Unpadded text for comparison.\")","        .border(.yellow)","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"The example above looks like this in iOS under typical conditions:"}],"type":"paragraph"},{"inlineContent":[],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"To control the amount of padding independently for each edge, use"},{"type":"text","text":" "},{"type":"codeVoice","code":"View\/padding(_:)-6pgqq"},{"type":"text","text":". To pad all outside edges of a view by a"},{"type":"text","text":" "},{"type":"text","text":"specified amount, use "},{"type":"codeVoice","code":"View\/padding(_:)-68shk"},{"type":"text","text":"."}],"type":"paragraph"}]}],"variants":[{"paths":["\/documentation\/openswiftui\/image\/padding(_:_:)"],"traits":[{"interfaceLanguage":"swift"}]}],"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Image"]]},"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/Image/padding(_:_:)":{"role":"symbol","fragments":[{"text"