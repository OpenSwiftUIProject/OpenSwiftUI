d":"text","text":"<"},{"kind":"genericParameter","text":"S"},{"kind":"text","text":">("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"style"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"S"},{"kind":"text","text":") -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"preciseIdentifier":"s:15OpenSwiftUICore4ViewP","kind":"typeIdentifier","text":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"kind":"text","text":" "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"S"},{"kind":"text","text":" : "},{"preciseIdentifier":"s:15OpenSwiftUICore10ShapeStyleP","kind":"typeIdentifier","text":"ShapeStyle","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle"},{"kind":"text","text":"\n"}],"platforms":["macOS"]}],"kind":"declarations"},{"parameters":[{"name":"style","content":[{"type":"paragraph","inlineContent":[{"text":"The color or pattern to use when filling in the","type":"text"},{"text":" ","type":"text"},{"text":"foreground elements. To indicate a specific value, use ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Color","type":"reference"},{"text":" or","type":"text"},{"text":" ","type":"text"},{"code":"ShapeStyle\/image(_:sourceRect:scale:)","type":"codeVoice"},{"text":", or one of the gradient","type":"text"},{"text":" ","type":"text"},{"text":"types, like","type":"text"},{"text":" ","type":"text"},{"code":"ShapeStyle\/linearGradient(colors:startPoint:endPoint:)","type":"codeVoice"},{"text":". To set a","type":"text"},{"text":" ","type":"text"},{"text":"style that’s relative to the containing view’s style, use one of the","type":"text"},{"text":" ","type":"text"},{"text":"semantic styles, like ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/primary","type":"reference"},{"text":".","type":"text"}]}]}],"kind":"parameters"},{"content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"text":"A view that uses the given foreground style.","type":"text"}]}],"kind":"content"},{"content":[{"type":"heading","anchor":"discussion","level":2,"text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"Use this method to style","type":"text"},{"text":" ","type":"text"},{"text":"foreground content like text, shapes, and template images","type":"text"},{"text":" ","type":"text"},{"text":"(including symbols):","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["HStack {","    Image(systemName: \"triangle.fill\")","    Text(\"Hello, world!\")","    RoundedRectangle(cornerRadius: 5)","        .frame(width: 40, height: 20)","}",".foregroundStyle(.teal)"]},{"type":"paragraph","inlineContent":[{"text":"The example above creates a row of ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/teal","type":"reference","isActive":true},{"text":" foreground","type":"text"},{"text":" ","type":"text"},{"text":"elements:","type":"text"}]},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"text":"You can use any style that conforms to the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle","type":"reference","isActive":true},{"text":" protocol,","type":"text"},{"text":" ","type":"text"},{"text":"like the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/teal","type":"reference","isActive":true},{"text":" color in the example above, or the","type":"text"},{"text":" ","type":"text"},{"code":"ShapeStyle\/linearGradient(colors:startPoint:endPoint:)","type":"codeVoice"},{"text":" gradient","type":"text"},{"text":" ","type":"text"},{"text":"shown below:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["Text(\"Gradient Text\")","    .font(.largeTitle)","    .foregroundStyle(","        .linearGradient(","            colors: [.yellow, .blue],","            startPoint: .top,","            endPoint: .bottom","        )","    )"]},{"type":"paragraph","inlineContent":[]},{"content":[{"inlineContent":[{"type":"text","text":"If you want to fill a single "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Shape"},{"type":"text","text":" instance with a style,"},{"type":"text","text":" "},{"type":"text","text":"use the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Shape\/fill(style:)"},{"type":"text","text":" shape modifier instead because it’s more"},{"type":"text","text":" "},{"type":"text","text":"efficient."}],"type":"paragraph"}],"type":"aside","style":"tip","name":"Tip"},{"type":"paragraph","inlineContent":[{"type":"text","text":"OpenSwiftUI creates a context-dependent render for a given style."},{"type":"text","text":" "},{"type":"text","text":"For example, a "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Color"},{"type":"text","text":" that you load from an asset catalog"},{"type":"text","text":" "},{"type":"text","text":"can have different light and dark appearances, while some styles"},{"type":"text","text":" "},{"type":"text","text":"also vary by platform."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Hierarchical foreground styles like "},{"code":"ShapeStyle\/secondary","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"don’t impose a style of their own, but instead modify other styles."},{"type":"text","text":" "},{"type":"text","text":"In particular, they modify the primary"},{"type":"text","text":" "},{"type":"text","text":"level of the current foreground style to the degree given by"},{"type":"text","text":" "},{"type":"text","text":"the hierarchical style’s name."},{"type":"text","text":" "},{"type":"text","text":"To find the current foreground style to modify, OpenSwiftUI looks for"},{"type":"text","text":" "},{"type":"text","text":"the innermost containing style that you apply with the"},{"type":"text","text":" "},{"code":"foregroundStyle(_:)","type":"codeVoice"},{"type":"text","text":" or the "},{"code":"View\/foregroundColor(_:)","type":"codeVoice"},{"type":"text","text":" modifier."},{"type":"text","text":" "},{"type":"text","text":"If you haven’t specified a style, OpenSwiftUI uses the default foreground"},{"type":"text","text":" "},{"type":"text","text":"style, as in the following example:"}]},{"type":"codeListing","syntax":"swift","code":["VStack(alignment: .leading) {","    Label(\"Primary\", systemImage: \"1.square.fill\")","    Label(\"Secondary\", systemImage: \"2.square.fill\")","        .foregroundStyle(.secondary)","}"]},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"text":"If you add a foreground style on the enclosing","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack","type":"reference","isActive":true},{"text":", the hierarchical styling responds accordingly:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["VStack(alignment: .leading) {","    Label(\"Primary\", systemImage: \"1.square.fill\")","    Label(\"Secondary\", systemImage: \"2.square.fill\")","        .foregroundStyle(.secondary)","}",".foregroundStyle(.blue)"]},{"type":"paragraph","inlineContent":[]},{"type":"paragraph","inlineContent":[{"text":"When you apply a custom style to a view, the view disables the vibrancy","type":"text"},{"text":" ","type":"text"},{"text":"effect for foreground elements in that view, or in any of its child","type":"text"},{"text":" ","type":"text"},{"text":"views, that it would otherwise gain from adding a background material","type":"text"},{"text":" ","type":"text"},{"text":"— for example, using the ","type":"text"},{"code":"View\/background(_:ignoresSafeAreaEdges:)","type":"codeVoice"},{"text":" ","type":"text"},{"text":"modifier. However, hierarchical styles applied to the default foreground","type":"text"},{"text":" ","type":"text"},{"text":"don’t disable vibrancy.","type":"text"}]}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/openswiftui\/divider\/foregroundstyle(_:)"]}],"kind":"symbol","schemaVersion":{"major":0,"minor":3,"patch":0},"abstract":[{"type":"text","text":"Sets a view’s foreground elements to use a given style."}],"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/VStack":{"type":"topic","kind":"symbol","abstract":[{"type":"text","text":"A view that arranges its subviews in a vertical line."}],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"VStack","kind":"identifier"}],"navigatorTitle":[{"text":"VStack","kind":"identifier"}],"title":"VStack","url":"\/documentation\/openswiftui\/vstack","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack","role":"symbol"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/ShapeStyle/primary":{"title":"primary","abstract":[{"type":"text","text":"A shape style that maps to the first level of the current content style."}],"url":"\/documentation\/openswiftui\/shapestyle\/primary","fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"primary","kind":"identifier"},{"text":": ","kind":"text"},{"text":"HierarchicalShapeStyle","kind":"typeIdentifier","preciseIdentifier":"s:15OpenSwiftUICore22HierarchicalShapeStyleV"}],"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ShapeStyle\/primary","kind":"symbol","type":"topic","role":"symbol","conformance":{"availabilityPrefix":[{"text":"Available when","type":"text"}],"constraints":[{"code":"Self","type":"codeVoice"},{"text":" is ","type":"text"},{"code":"HierarchicalShapeStyle","type":"codeVoice"},{"text":".",