t"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"tag","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"includeOptional","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"},{"text":" = true) -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP"},{"text":" ","kind":"text"},{"text":"where","kind":"keyword"},{"text":" ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"Hashable","kind":"typeIdentifier","preciseIdentifier":"s:SH"},{"text":"\n","kind":"text"}],"platforms":["macOS"]}]},{"kind":"parameters","parameters":[{"content":[{"inlineContent":[{"type":"text","text":"A "},{"type":"reference","isActive":true,"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable"},{"type":"text","text":" "},{"type":"text","text":"value to use as the view’s tag."}],"type":"paragraph"}],"name":"tag"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"If the tag value for "},{"type":"codeVoice","code":"Optional<V>"},{"type":"text","text":" should"},{"type":"text","text":" "},{"type":"text","text":"also be set."}]}],"name":"includeOptional"}]},{"kind":"content","content":[{"type":"heading","level":2,"text":"Return Value","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A view with the specified tag set."}]}]},{"kind":"content","content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"inlineContent":[{"text":"Use this modifier to differentiate among certain selectable views,","type":"text"},{"text":" ","type":"text"},{"text":"like the possible values of a ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" or the tabs of a ","type":"text"},{"code":"TabView","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"Tag values can be of any type that conforms to the","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable"},{"text":" protocol.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"This modifier will write the tag value for the type ","type":"text"},{"code":"V","type":"codeVoice"},{"text":", as well as","type":"text"},{"text":" ","type":"text"},{"code":"Optional<V>","type":"codeVoice"},{"text":" if ","type":"text"},{"code":"includeOptional","type":"codeVoice"},{"text":" is enabled. Containers checking for","type":"text"},{"text":" ","type":"text"},{"text":"tags of either type will see the value as set.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"In the example below, the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach"},{"type":"text","text":" loop in the "},{"code":"Picker","type":"codeVoice"},{"type":"text","text":" view"},{"type":"text","text":" "},{"type":"text","text":"builder iterates over the "},{"code":"Flavor","type":"codeVoice"},{"text":" enumeration. It extracts the string","type":"text"},{"text":" ","type":"text"},{"text":"value of each enumeration element for use in constructing the row","type":"text"},{"text":" ","type":"text"},{"text":"label, and uses the enumeration value as input to the ","type":"text"},{"code":"tag(_:)","type":"codeVoice"},{"text":" ","type":"text"},{"text":"modifier.","type":"text"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["struct FlavorPicker: View {","    enum Flavor: String, CaseIterable, Identifiable {","        case chocolate, vanilla, strawberry","        var id: Self { self }","    }","","    @State private var selectedFlavor: Flavor? = nil","","    var body: some View {","        Picker(\"Flavor\", selection: $selectedFlavor) {","            ForEach(Flavor.allCases) { flavor in","                Text(flavor.rawValue)","                    .tag(flavor)","            }","        }","    }","}"]},{"inlineContent":[{"type":"text","text":"The selection type of the "},{"code":"Picker","type":"codeVoice"},{"type":"text","text":" is an "},{"code":"Optional<Flavor>","type":"codeVoice"},{"type":"text","text":" and so it"},{"type":"text","text":" "},{"type":"text","text":"will look for tags on its contents of "},{"code":"Optional<Flavor>","type":"codeVoice"},{"type":"text","text":"type. Since the"},{"type":"text","text":" "},{"type":"text","text":"tag modifier defaults to having "},{"code":"includeOptional","type":"codeVoice"},{"type":"text","text":" enabled, even though"},{"type":"text","text":" "},{"type":"text","text":"the tag for each option is a non-optional "},{"code":"Flavor","type":"codeVoice"},{"type":"text","text":", the tag modifier"},{"type":"text","text":" "},{"type":"text","text":"writes values for both the non-optional, and optional versions of the"},{"type":"text","text":" "},{"type":"text","text":"value, allowing the contents to be selectable by the "},{"code":"Picker","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"text":"A ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true,"type":"reference"},{"text":" automatically applies a default tag to each enumerated","type":"text"},{"text":" ","type":"text"},{"text":"view using the ","type":"text"},{"type":"codeVoice","code":"id"},{"text":" parameter of the corresponding element. If","type":"text"},{"text":" ","type":"text"},{"text":"the element’s ","type":"text"},{"type":"codeVoice","code":"id"},{"text":" parameter and the picker’s ","type":"text"},{"type":"codeVoice","code":"selection"},{"text":" input","type":"text"},{"text":" ","type":"text"},{"text":"have exactly the same type, or the same type but optional, you can omit","type":"text"},{"text":" ","type":"text"},{"text":"the explicit tag modifier.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"To see examples that don’t require an explicit tag, see ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"}]}],"identifier":{"url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Slider\/tag(_:includeOptional:)","interfaceLanguage":"swift"},"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Slider"]]},"abstract":[{"text":"Sets the unique tag value of this view.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/openswiftui\/slider\/tag(_:includeoptional:)"]}],"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/ForEach":{"url":"\/documentation\/openswiftui\/foreach","title":"ForEach","abstract":[{"text":"A structure that computes views on demand from an underlying collection of","type":"text"},{"text":" ","type":"text"},{"text":"identified data.","type":"text"}],"kind":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"ForEach","kind":"identifier"}],"navigatorTitle":[{"text":"ForEach","kind":"identifier"}],"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","type":"topic","role":"symbol"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/Slider/tag(_:includeOptional:)":{"title":"tag(_:includeOptional:)","kind":"symbol","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"tag"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"V"},{"kind":"text","text":">("},{"kind":"typeIdentifier","text":"V"},{"kind":"text","text":", "},{"kind":"externalParam","text":"includeOptional"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"kind":"te