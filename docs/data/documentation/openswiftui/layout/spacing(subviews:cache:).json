enSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout"]]},"abstract":[{"text":"Returns the preferred spacing values of the composite view.","type":"text"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"spacing"},{"kind":"text","text":"("},{"kind":"externalParam","text":"subviews"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Self"},{"kind":"text","text":"."},{"kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/Subviews","text":"Subviews","preciseIdentifier":"s:15OpenSwiftUICore6LayoutP8Subviewsa"},{"kind":"text","text":", "},{"kind":"externalParam","text":"cache"},{"kind":"text","text":": "},{"kind":"keyword","text":"inout"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"Self"},{"kind":"text","text":"."},{"kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/Cache","text":"Cache","preciseIdentifier":"s:15OpenSwiftUICore6LayoutP5CacheQa"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ViewSpacing","text":"ViewSpacing","preciseIdentifier":"s:15OpenSwiftUICore11ViewSpacingV"}],"languages":["swift"],"platforms":["macOS"]}]},{"parameters":[{"name":"subviews","content":[{"type":"paragraph","inlineContent":[{"text":"A collection of proxy instances that represent the","type":"text"},{"text":" ","type":"text"},{"text":"views that the container arranges. You can use the proxies in the","type":"text"},{"text":" ","type":"text"},{"text":"collection to get information about the subviews as you determine","type":"text"},{"text":" ","type":"text"},{"text":"how much spacing the container prefers around it.","type":"text"}]}]},{"name":"cache","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Optional storage for calculated data that you can share among"},{"type":"text","text":" "},{"type":"text","text":"the methods of your custom layout container. See"},{"type":"text","text":" "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/makeCache(subviews:)","type":"reference","isActive":true},{"type":"text","text":" for details."}]}]}],"kind":"parameters"},{"content":[{"level":2,"anchor":"return-value","text":"Return Value","type":"heading"},{"inlineContent":[{"text":"A ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ViewSpacing","type":"reference"},{"text":" instance that describes the preferred","type":"text"},{"text":" ","type":"text"},{"text":"spacing around the container view.","type":"text"}],"type":"paragraph"}],"kind":"content"},{"content":[{"type":"heading","anchor":"discussion","level":2,"text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"Implement this method to provide custom spacing preferences","type":"text"},{"text":" ","type":"text"},{"text":"for a layout container. The value you return affects","type":"text"},{"text":" ","type":"text"},{"text":"the spacing around the container, but it doesn’t affect how the","type":"text"},{"text":" ","type":"text"},{"text":"container arranges subviews relative to one another inside the","type":"text"},{"text":" ","type":"text"},{"text":"container.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Create a custom "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ViewSpacing","isActive":true},{"type":"text","text":" instance for your container by"},{"type":"text","text":" "},{"type":"text","text":"initializing one with default values, and then merging that with"},{"type":"text","text":" "},{"type":"text","text":"spacing instances of certain subviews. For example, if you define"},{"type":"text","text":" "},{"type":"text","text":"a basic vertical stack that places subviews in a column, you could"},{"type":"text","text":" "},{"type":"text","text":"use the spacing preferences of the subview edges that make"},{"type":"text","text":" "},{"type":"text","text":"contact with the container’s edges:"}]},{"type":"codeListing","code":["extension BasicVStack {","    func spacing(subviews: Subviews, cache: inout ()) -> ViewSpacing {","        var spacing = ViewSpacing()","","        for index in subviews.indices {","            var edges: Edge.Set = [.leading, .trailing]","            if index == 0 { edges.formUnion(.top) }","            if index == subviews.count - 1 { edges.formUnion(.bottom) }","            spacing.formUnion(subviews[index].spacing, edges: edges)","        }","","        return spacing","    }","}"],"syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"In the above example, the first and last subviews contribute to the","type":"text"},{"text":" ","type":"text"},{"text":"spacing above and below the container, respectively, while all subviews","type":"text"},{"text":" ","type":"text"},{"text":"affect the spacing on the leading and trailing edges.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"If you don’t implement this method, the protocol provides a default","type":"text"},{"text":" ","type":"text"},{"text":"implementation that merges the spacing preferences across all subviews on all edges.","type":"text"}]}],"kind":"content"}],"defaultImplementationsSections":[{"title":"Layout Implementations","anchor":"Layout-Implementations","identifiers":["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/spacing(subviews:cache:)-2dxih"]}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Layout\/spacing(subviews:cache:)"},"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/Layout":{"abstract":[{"text":"A type that defines the geometry of a collection of views.","type":"text"}],"url":"\/documentation\/openswiftui\/layout","title":"Layout","fragments":[{"text":"protocol","kin