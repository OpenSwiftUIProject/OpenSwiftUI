":"topic"},"doc://OpenSwiftUI/documentation/OpenSwiftUI":{"type":"topic","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","url":"\/documentation\/openswiftui","title":"OpenSwiftUI","kind":"symbol","role":"collection","abstract":[{"type":"text","text":"Declare the user interface and behavior for your app on every platform."}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/View":{"title":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","type":"topic","kind":"symbol","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","kind":"identifier"}],"role":"symbol","url":"\/documentation\/openswiftui\/view","abstract":[{"text":"A type that represents part of your app’s user interface and provides","type":"text"},{"text":" ","type":"text"},{"text":"modifiers that you use to configure views.","type":"text"}],"navigatorTitle":[{"text":"View","kind":"identifier"}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/View/userActivity(_:isActive:_:)":{"type":"topic","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/userActivity(_:isActive:_:)","kind":"symbol","role":"symbol","abstract":[{"type":"text","text":"Advertises a user activity type."}],"url":"\/documentation\/openswiftui\/view\/useractivity(_:isactive:_:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"userActivity"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"kind":"text","text":", "},{"kind":"externalParam","text":"isActive"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"kind":"text","text":", ("},{"kind":"typeIdentifier","text":"NSUserActivity","preciseIdentifier":"c:objc(cs)NSUserActivity"},{"kind":"text","text":") -> ()) -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP"}]