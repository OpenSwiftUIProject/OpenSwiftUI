.com\")!","@State private var message = \"Loading...\"","","var body: some View {","    Text(message)","        .task {","            do {","                var receivedLines = [String]()","                for try await line in url.lines {","                    receivedLines.append(line)","                    message = \"Received \\(receivedLines.count) lines\"","                }","            } catch {","                message = \"Failed to load\"","            }","        }","}"]},{"type":"paragraph","inlineContent":[{"text":"This example uses the","type":"text"},{"text":" ","type":"text"},{"identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/url\/3767315-lines","isActive":true,"type":"reference"},{"text":" ","type":"text"},{"text":"method to get the content stored at the specified","type":"text"},{"text":" ","type":"text"},{"identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/url","isActive":true,"type":"reference"},{"text":" as an","type":"text"},{"text":" ","type":"text"},{"text":"asynchronous sequence of strings. When each new line arrives, the body","type":"text"},{"text":" ","type":"text"},{"text":"of the ","type":"text"},{"code":"for","type":"codeVoice"},{"text":"-","type":"text"},{"code":"await","type":"codeVoice"},{"text":"-","type":"text"},{"code":"in","type":"codeVoice"},{"text":" loop stores the line in an array of strings","type":"text"},{"text":" ","type":"text"},{"text":"and updates the content of the text view to report the latest line","type":"text"},{"text":" ","type":"text"},{"text":"count.","type":"text"}]}],"kind":"content"}],"identifier":{"url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/task(priority:_:)","interfaceLanguage":"swift"},"metadata":{"extendedModule":"OpenSwiftUICore","title":"task(priority:_:)","externalID":"s:15OpenSwiftUICore4ViewP0aB2UIE4task8priority_QrScP_yyYaYbctF","role":"symbol","symbolKind":"method","modules":[{"relatedModules":["OpenSwiftUICore"],"name":"OpenSwiftUI"}