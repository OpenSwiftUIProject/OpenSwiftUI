ctive"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"kind":"text","text":", ("},{"kind":"typeIdentifier","text":"NSUserActivity","preciseIdentifier":"c:objc(cs)NSUserActivity"},{"kind":"text","text":") -> ()) -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP"}],"title":"userActivity(_:isActive:_:)"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/View/onContinueUserActivity(_:perform:)":{"url":"\/documentation\/openswiftui\/view\/oncontinueuseractivity(_:perform:)","title":"onContinueUserActivity(_:perform:)","role":"symbol","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"onContinueUserActivity"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"kind":"text","text":", "},{"kind":"externalParam","text":"perform"},{"kind":"text","text":": ("},{"kind":"typeIdentifier","text":"NSUserActivity","preciseIdentifier":"c:objc(cs)NSUserActivity"},{"kind":"text","text":") -> ()) -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP"}],"kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Registers a handler to invoke in response to a user activity that your"},{"type":"text","text":" "},{"type":"text","text":"app receives."}],"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/onContinueUserActivity(_:perform:)"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/View":{"title":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","type":"topic","kind":"symbol","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","kind":"identifier"}],"role":"symbol","url":"\/documentation\/openswiftui\/view","abstract":[{"text":"A type that represents part of your app