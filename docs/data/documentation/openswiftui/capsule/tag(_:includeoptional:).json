{"variants":[{"paths":["\/documentation\/openswiftui\/capsule\/tag(_:includeoptional:)"],"traits":[{"interfaceLanguage":"swift"}]}],"kind":"symbol","sections":[],"schemaVersion":{"patch":0,"minor":3,"major":0},"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Capsule"]]},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Capsule\/tag(_:includeOptional:)"},"abstract":[{"text":"Sets the unique tag value of this view.","type":"text"}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"nonisolated","kind":"attribute"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"tag","kind":"identifier"},{"text":"<","kind":"text"},{"text":"V","kind":"genericParameter"},{"text":">(","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"tag","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"includeOptional","kind":"externalParam"},{"text":": ","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"},{"text":" = true) -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","kind":"typeIdentifier","preciseIdentifier":"s:15OpenSwiftUICore4ViewP","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"text":" ","kind":"text"},{"text":"where","kind":"keyword"},{"text":" ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"Hashable","kind":"typeIdentifier","preciseIdentifier":"s:SH"},{"text":"\n","kind":"text"}],"languages":["swift"],"platforms":["macOS"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"tag","content":[{"inlineContent":[{"type":"text","text":"A "},{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","isActive":true,"type":"reference"},{"type":"text","text":" "},{"type":"text","text":"value to use as the view’s tag."}],"type":"paragraph"}]},{"name":"includeOptional","content":[{"inlineContent":[{"type":"text","text":"If the tag value for "},{"type":"codeVoice","code":"Optional<V>"},{"type":"text","text":" should"},{"type":"text","text":" "},{"type":"text","text":"also be set."}],"type":"paragraph"}]}]},{"content":[{"anchor":"return-value","text":"Return Value","level":2,"type":"heading"},{"inlineContent":[{"text":"A view with the specified tag set.","type":"text"}],"type":"paragraph"}],"kind":"content"},{"content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"Use this modifier to differentiate among certain selectable views,","type":"text"},{"text":" ","type":"text"},{"text":"like the possible values of a ","type":"text"},{"type":"codeVoice","code":"Picker"},{"text":" or the tabs of a ","type":"text"},{"type":"codeVoice","code":"TabView"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"Tag values can be of any type that conforms to the","type":"text"},{"text":" ","type":"text"},{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","isActive":true,"type":"reference"},{"text":" protocol.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"This modifier will write the tag value for the type "},{"type":"codeVoice","code":"V"},{"type":"text","text":", as well as"},{"type":"text","text":" "},{"type":"codeVoice","code":"Optional<V>"},{"type":"text","text":" if "},{"type":"codeVoice","code":"includeOptional"},{"type":"text","text":" is enabled. Containers checking for"},{"type":"text","text":" "},{"type":"text","text":"tags of either type will see the value as set."}]},{"type":"paragraph","inlineContent":[{"text":"In the example below, the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true,"type":"reference"},{"text":" loop in the ","type":"text"},{"type":"codeVoice","code":"Picker"},{"text":" view","type":"text"},{"text":" ","type":"text"},{"text":"builder iterates over the ","type":"text"},{"type":"codeVoice","code":"Flavor"},{"text":" enumeration. It extracts the string","type":"text"},{"text":" ","type":"text"},{"text":"value of each enumeration element for use in constructing the row","type":"text"},{"text":" ","type":"text"},{"text":"label, and uses the enumeration value as input to the ","type":"text"},{"type":"codeVoice","code":"tag(_:)"},{"text":" ","type":"text"},{"text":"modifier.","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["struct FlavorPicker: View {","    enum Flavor: String, CaseIterable, Identifiable {","        case chocolate, vanilla, strawberry","        var id: Self { self }","    }","","    @State private var selectedFlavor: Flavor? = nil","","    var body: some View {","        Picker(\"Flavor\", selection: $selectedFlavor) {","            ForEach(Flavor.allCases) { flavor in","                Text(flavor.rawValue)","                    .tag(flavor)","            }","        }","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The selection type of the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":" is an "},{"type":"codeVoice","code":"Optional<Flavor>"},{"type":"text","text":" and so it"},{"type":"text","text":" "},{"type":"text","text":"will look for tags on its contents of "},{"type":"codeVoice","code":"Optional<Flavor>"},{"type":"text","text":"type. Since the"},{"type":"text","text":" "},{"type":"text","text":"tag modifier defaults to having "},{"type":"codeVoice","code":"includeOptional"},{"type":"text","text":" enabled, even though"},{"type":"text","text":" "},{"type":"text","text":"the tag for each option is a non-optional "},{"type":"codeVoice","code":"Flavor"},{"type":"text","text":", the tag modifier"},{"type":"text","text":" "},{"type":"text","text":"writes values for both the non-optional, and optional versions of the"},{"type":"text","text":" "},{"type":"text","text":"value, allowing the contents to be selectable by the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"A "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true},{"type":"text","text":" automatically applies a default tag to each enumerated"},{"type":"text","text":" "},{"type":"text","text":"view using the "},{"type":"codeVoice","code":"id"},{"type":"text","text":" parameter of the corresponding element. If"},{"type":"text","text":" "},{"type":"text","text":"the element’s "},{"type":"codeVoice","code":"id"},{"type":"text","text":" parameter and the picker’s "},{"type":"codeVoice","code":"selection"},{"type":"text","text":" input"},{"type":"text","text":" "},{"type":"text","text":"have exactly the same type, or the same type but optional, you can omit"},{"type":"text","text":" "},{"type":"text","text":"the explicit tag modifier."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"To see examples that don’t require an explicit tag, see "},{"code":"Picker","type":"codeVoice"},{"type":"text","text":"."}]}],"kind":"content"}],"metadata":{"modules":[{"name":"OpenSwiftUI","relatedModules":["OpenSwiftUICore"]}],"role":"symbol","symbolKind":"method","extendedModule":"OpenSwiftUICore","roleHeading":"Instance Method","title":"tag(_:includeOptional:)","platforms":[{"introducedAt":"13.0","deprecated":false,"unavailable":false,"name":"iOS","beta":false},{"name":"macOS","beta":false,"unavailable":false,"introducedAt":"10.15","deprecated":false},{"name":"tvOS","beta":false,"unavailable":false,"introducedAt":"13.0","deprecated":false},{"name":"visionOS","beta":false,"unavailable":false,"introducedAt":"1.0","deprecated":false},{"name":"watchOS","beta":false,"unavailable":false,"introducedAt":"6.0","deprecated":false}],"externalID":"s:15OpenSwiftUICor