OpenSwiftUI\/Text"},{"type":"text","text":" element, even though all of their"},{"type":"text","text":" "},{"type":"text","text":"other attributes (font, font size and character count) are the same."}],"type":"paragraph"},{"inlineContent":[],"type":"paragraph"},{"inlineContent":[{"text":"A parent layout offers the child views with the highest layout priority","type":"text"},{"text":" ","type":"text"},{"text":"all the space offered to the parent minus the minimum space required for","type":"text"},{"text":" ","type":"text"},{"text":"all its lower-priority children.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/openswiftui\/placeholdercontentview\/layoutpriority(_:)"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PlaceholderContentView\/layoutPriority(_:)"},"hierarchy":{"paths":[["doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI","doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PlaceholderContentView"]]},"abstract":[{"text":"Sets the priority by which a parent layout should apportion space to","type":"text"},{"text":" ","type":"text"},{"text":"this child.","type":"text"}],"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/PlaceholderContentView":{"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"PlaceholderContentView","kind":"identifier"}],"url":"\/documentation\/openswiftui\/placeholdercontentview","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/PlaceholderContentView","role":"symbol","navigatorTitle":[{"text":"PlaceholderContentView","kind":"identifier"}],"title":"PlaceholderContentView","kind":"symbol","abstract":[{"type":"text","text":"A placeholder used to construct an inline modifier, transition, or other"},{"type":"text","text":" "},{"type":"text","text":"helper type."}],"type":"topic"},"doc://Open