"}],"kind":"symbol","schemaVersion":{"major":0,"minor":3,"patch":0},"primaryContentSections":[{"declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"text":"nonisolated","kind":"attribute"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"multilineTextAlignment","kind":"identifier"},{"text":"(","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"alignment","kind":"internalParam"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:15OpenSwiftUICore13TextAlignmentO","text":"TextAlignment","kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/TextAlignment"},{"text":") -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"preciseIdentifier":"s:15OpenSwiftUICore4ViewP","text":"View","kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"text":"\n","kind":"text"}]}],"kind":"declarations"},{"parameters":[{"name":"alignment","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A value that you use to align multiple lines of"},{"type":"text","text":" "},{"type":"text","text":"text within a view."}]}]}],"kind":"parameters"},{"content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"inlineContent":[{"type":"text","text":"A view that aligns the lines of multiline "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","isActive":true},{"type":"text","text":" instances"},{"type":"text","text":" "},{"type":"text","text":"it contains."}],"type":"paragraph"}],"kind":"content"},{"content":[{"level":2,"anchor":"discussion","text":"Discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"Use this modifier to set an alignment for a multiline block of text."},{"type":"text","text":" "},{"type":"text","text":"For example, the modifier centers the contents of the following"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","isActive":true},{"type":"text","text":" view:"}],"type":"paragraph"},{"syntax":"swift","code":["Text(\"This is a block of text that shows up in a text element as multiple lines.\\(\"\\n\") Here we have chosen to center this text.\")","    .frame(width: 200)","    .multilineTextAlignment(.center)"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"The text in the above example spans more than one line because:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"text","text":"The newline character introduces a line break."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"The frame modifier limits the space available to the text view, and","type":"text"},{"text":" ","type":"text"},{"text":"by default a text view wraps lines that don’t fit in the available","type":"text"},{"text":" ","type":"text"},{"text":"width. As a result, the text before the explicit line break wraps to","type":"text"},{"text":" ","type":"text"},{"text":"three lines, and the text after uses two lines.","type":"text"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"text":"The modifier applies the alignment to the all the lines of text in","type":"text"},{"text":" ","type":"text"},{"text":"the view, regardless of why wrapping occurs:","type":"text"}],"type":"paragraph"},{"inlineContent":[],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The modifier has no effect on a "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","type":"reference","isActive":true},{"type":"text","text":" view that contains only one"},{"type":"text","text":" "},{"type":"text","text":"line of text, because a text view has a width that exactly matches the"},{"type":"text","text":" "},{"type":"text","text":"width of its widest line. If you want to align an entire text view"},{"type":"text","text":" "},{"type":"text","text":"rather than its contents, set the aligment of its container, like a"},{"type":"text","text":" "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack","type":"reference","isActive":true},{"type":"text","text":" or a frame that you create with the"},{"type":"text","text":" "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/frame(minWidth:idealWidth:maxWidth:minHeight:idealHeight:maxHeight:alignment:)","type":"reference","isActive":true},{"type":"text","text":" "},{"type":"text","text":"modifier."}],"type":"paragraph"},{"style":"note","name":"Note","content":[{"inlineContent":[{"text":"You can use this modifier to control the alignment of a ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text","isActive":true,"type":"reference"},{"text":" ","type":"text"},{"text":"view that you create with the ","type":"text"},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Text\/init(_:style:)","isActive":true,"type":"reference"},{"text":" initializer","type":"text"},{"text":" ","type":"text"},{"text":"to display localized dates and times, including when the view uses","type":"text"},{"text":" ","type":"text"},{"text":"only a single line, but only when that view appears in a widget.","type":"text"}],"type":"paragraph"}],"type":"aside"},{"inlineContent":[{"type":"text","text":"The modifier also affects the content alignment of other text container"},{"type":"text","text":" "},{"type":"text","text":"types, like "},{"code":"TextEditor","type":"codeVoice"},{"type":"text","text":" and "},{"code":"TextField","type":"codeVoice"},{"type":"text","text":". In those cases, the"},{"type":"text","text":" "},{"type":"text","text":"modifier sets the alignment even when the view contains only a single"},{"type":"text","text":" "},{"type":"text","text":"line because view’s width isn’t dictated by the width of the text it"},{"type":"text","text":" "},{"type":"text","text":"contains."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The modifier operates by setting the"},{"type":"text","text":" "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/EnvironmentValues\/multilineTextAlignment","type":"reference","isActive":true},{"type":"text","text":" value in the environment,"},{"type":"text","text":" "},{"type":"text","text":"so it affects all the text containers in the modified view hierarchy."},{"type":"text","text":" "},{"type":"text","text":"For example, you can apply the modifier to a "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/VStack","type":"reference","isActive":true},{"type":"text","text":" to"},{"type":"text","text":" "},{"type":"text","text":"configure all the text views inside the stack."}],"type":"paragraph"}],"kind":"content"}],"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/Spacer/multilineTextAlignment(_:)":{"abstract":[{"type":"text","text":"Sets the alignment of a text view that contains multiple lines of text."}],"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Spacer\/multilineTextAlignment(_:)","url":"\/documentation\/openswiftui\/spacer\/multilinetextalignment(_:)","kind":"symbol","role":"symbol","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"multilineTextAlignment"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"TextAlignment","preciseIdentifier":"s:15OpenSwiftUICore13TextAlignmentO"},{"kind":"text","text":") -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"View","preciseIdentifier":"s:15OpenSwiftUICore4ViewP"}],"type":"topic","title":"multilineTextAlignment(_:)"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/Text/init(_:style:)":{"type":"topic","title":"init(_:style:)","role":"symbol","url":"\/documentation\/openswiftui\/text\/init(_:style:)","fragments":[{"text":"init","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"s:10Foundation4DateV","text":"Date"