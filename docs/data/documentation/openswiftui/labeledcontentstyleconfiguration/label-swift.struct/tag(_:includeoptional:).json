/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/LabeledContentStyleConfiguration\/Label-swift.struct\/tag(_:includeOptional:)"},"primaryContentSections":[{"declarations":[{"tokens":[{"text":"nonisolated","kind":"attribute"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"tag","kind":"identifier"},{"text":"<","kind":"text"},{"text":"V","kind":"genericParameter"},{"text":">(","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"tag","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"includeOptional","kind":"externalParam"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"},{"text":" = true) -> ","kind":"text"},{"text":"some","kind":"keyword"},{"text":" ","kind":"text"},{"preciseIdentifier":"s:15OpenSwiftUICore4ViewP","text":"View","kind":"typeIdentifier","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"text":" ","kind":"text"},{"text":"where","kind":"keyword"},{"text":" ","kind":"text"},{"text":"V","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"preciseIdentifier":"s:SH","text":"Hashable","kind":"typeIdentifier"},{"text":"\n","kind":"text"}],"platforms":["macOS"],"languages":["swift"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A "},{"isActive":true,"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","type":"reference"},{"type":"text","text":" "},{"type":"text","text":"value to use as the view’s tag."}]}],"name":"tag"},{"content":[{"type":"paragraph","inlineContent":[{"text":"If the tag value for ","type":"text"},{"code":"Optional<V>","type":"codeVoice"},{"text":" should","type":"text"},{"text":" ","type":"text"},{"text":"also be set.","type":"text"}]}],"name":"includeOptional"}]},{"kind":"content","content":[{"text":"Return Value","level":2,"type":"heading","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A view with the specified tag set."}]}]},{"kind":"content","content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Use this modifier to differentiate among certain selectable views,"},{"type":"text","text":" "},{"type":"text","text":"like the possible values of a "},{"code":"Picker","type":"codeVoice"},{"type":"text","text":" or the tabs of a "},{"code":"TabView","type":"codeVoice"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Tag values can be of any type that conforms to the"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable"},{"type":"text","text":" protocol."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"This modifier will write the tag value for the type "},{"type":"codeVoice","code":"V"},{"type":"text","text":", as well as"},{"type":"text","text":" "},{"type":"codeVoice","code":"Optional<V>"},{"type":"text","text":" if "},{"type":"codeVoice","code":"includeOptional"},{"type":"text","text":" is enabled. Containers checking for"},{"type":"text","text":" "},{"type":"text","text":"tags of either type will see the value as set."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"In the example below, the "},{"type":"reference","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true},{"type":"text","text":" loop in the "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":" view"},{"type":"text","text":" "},{"type":"text","text":"builder iterates over the "},{"type":"codeVoice","code":"Flavor"},{"type":"text","text":" enumeration. It extracts the string"},{"type":"text","text":" "},{"type":"text","text":"value of each enumeration element for use in constructing the row"},{"type":"text","text":" "},{"type":"text","text":"label, and uses the enumeration value as input to the "},{"type":"codeVoice","code":"tag(_:)"},{"type":"text","text":" "},{"type":"text","text":"modifier."}]},{"code":["struct FlavorPicker: View {","    enum Flavor: String, CaseIterable, Identifiable {","        case chocolate, vanilla, strawberry","        var id: Self { self }","    }","","    @State private var selectedFlavor: Flavor? = nil","","    var body: some View {","        Picker(\"Flavor\", selection: $selectedFlavor) {","            ForEach(Flavor.allCases) { flavor in","                Text(flavor.rawValue)","                    .tag(flavor)","            }","        }","    }","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"text":"The selection type of the ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" is an ","type":"text"},{"code":"Optional<Flavor>","type":"codeVoice"},{"text":" and so it","type":"text"},{"text":" ","type":"text"},{"text":"will look for tags on its contents of ","type":"text"},{"code":"Optional<Flavor>","type":"codeVoice"},{"text":"type. Since the","type":"text"},{"text":" ","type":"text"},{"text":"tag modifier defaults to having ","type":"text"},{"code":"includeOptional","type":"codeVoice"},{"text":" enabled, even though","type":"text"},{"text":" ","type":"text"},{"text":"the tag for each option is a non-optional ","type":"text"},{"code":"Flavor","type":"codeVoice"},{"text":", the tag modifier","type":"text"},{"text":" ","type":"text"},{"text":"writes values for both the non-optional, and optional versions of the","type":"text"},{"text":" ","type":"text"},{"text":"value, allowing the contents to be selectable by the ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":".","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"A "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true,"type":"reference"},{"type":"text","text":" automatically applies a default tag to each enumerated"},{"type":"text","text":" "},{"type":"text","text":"view using the "},{"type":"codeVoice","code":"id"},{"type":"text","text":" parameter of the corresponding element. If"},{"type":"text","text":" "},{"type":"text","text":"the element’s "},{"type":"codeVoice","code":"id"},{"type":"text","text":" parameter and the picker’s "},{"type":"codeVoice","code":"selection"},{"type":"text","text":" input"},{"type":"text","text":" "},{"type":"text","text":"have exactly the same type, or the same type but optional, you can omit"},{"type":"text","text":" "},{"type":"text","text":"the explicit tag modifier."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"To see examples that don’t require an explicit tag, see "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":"."}]}]}],"abstract":[{"text":"Sets the unique tag value of this view.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/openswiftui\/labeledcontentstyleconfiguration\/label-swift.struct\/tag(_:includeoptional:)"]}],"references":{"doc://OpenSwiftUI/documentation/OpenSwiftUI/View":{"title":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View","type":"topic","kind":"symbol","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"View","kind":"identifier"}],"role":"symbol","url":"\/documentation\/openswiftui\/view","abstract":[{"text":"A type that represents part of your app’s user interface and provides","type":"text"},{"text":" ","type":"text"},{"text":"modifiers that you use to configure views.","type":"text"}],"navigatorTitle":[{"text":"View","kind":"identifier"}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/LabeledContentStyleConfiguration/Label-swift.struct":{"abstract":[{"type":"text","text":"A type-erased label of a labeled content instance."}],"title":"LabeledContentStyleConfiguration.Label","fragments":[{"text":"struct","kind":"keyword"},{"tex