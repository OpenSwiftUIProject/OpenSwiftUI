maryContentSections":[{"declarations":[{"tokens":[{"kind":"attribute","text":"nonisolated"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"tag"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"V"},{"kind":"text","text":">("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"tag"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"V"},{"kind":"text","text":", "},{"kind":"externalParam","text":"includeOptional"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"kind":"text","text":" = true) -> "},{"kind":"keyword","text":"some"},{"kind":"text","text":" "},{"preciseIdentifier":"s:15OpenSwiftUICore4ViewP","kind":"typeIdentifier","text":"View","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View"},{"kind":"text","text":" "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"V"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"Hashable","preciseIdentifier":"s:SH"},{"kind":"text","text":"\n"}],"platforms":["macOS"],"languages":["swift"]}],"kind":"declarations"},{"parameters":[{"name":"tag","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A "},{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","type":"reference","isActive":true},{"type":"text","text":" "},{"type":"text","text":"value to use as the view’s tag."}]}]},{"name":"includeOptional","content":[{"inlineContent":[{"type":"text","text":"If the tag value for "},{"type":"codeVoice","code":"Optional<V>"},{"type":"text","text":" should"},{"type":"text","text":" "},{"type":"text","text":"also be set."}],"type":"paragraph"}]}],"kind":"parameters"},{"content":[{"anchor":"return-value","type":"heading","text":"Return Value","level":2},{"type":"paragraph","inlineContent":[{"text":"A view with the specified tag set.","type":"text"}]}],"kind":"content"},{"content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"text":"Use this modifier to differentiate among certain selectable views,","type":"text"},{"text":" ","type":"text"},{"text":"like the possible values of a ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" or the tabs of a ","type":"text"},{"code":"TabView","type":"codeVoice"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"Tag values can be of any type that conforms to the","type":"text"},{"text":" ","type":"text"},{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","isActive":true,"type":"reference"},{"text":" protocol.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"This modifier will write the tag value for the type "},{"type":"codeVoice","code":"V"},{"type":"text","text":", as well as"},{"type":"text","text":" "},{"type":"codeVoice","code":"Optional<V>"},{"type":"text","text":" if "},{"type":"codeVoice","code":"includeOptional"},{"type":"text","text":" is enabled. Containers checking for"},{"type":"text","text":" "},{"type":"text","text":"tags of either type will see the value as set."}],"type":"paragraph"},{"inlineContent":[{"text":"In the example below, the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","type":"reference"},{"text":" loop in the ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" view","type":"text"},{"text":" ","type":"text"},{"text":"builder iterates over the ","type":"text"},{"code":"Flavor","type":"codeVoice"},{"text":" enumeration. It extracts the string","type":"text"},{"text":" ","type":"text"},{"text":"value of each enumeration element for use in constructing the row","type":"text"},{"text":" ","type":"text"},{"text":"label, and uses the enumeration value as input to the ","type":"text"},{"code":"tag(_:)","type":"codeVoice"},{"text":" ","type":"text"},{"text":"modifier.","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["struct FlavorPicker: View {","    enum Flavor: String, CaseIterable, Identifiable {","        case chocolate, vanilla, strawberry","        var id: Self { self }","    }","","    @State private var selectedFlavor: Flavor? = nil","","    var body: some View {","        Picker(\"Flavor\", selection: $selectedFlavor) {","            ForEach(Flavor.allCases) { flavor in","                Text(flavor.rawValue)","                    .tag(flavor)","            }","        }","    }","}"]},{"inlineContent":[{"text":"The selection type of the ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":" is an ","type":"text"},{"code":"Optional<Flavor>","type":"codeVoice"},{"text":" and so it","type":"text"},{"text":" ","type":"text"},{"text":"will look for tags on its contents of ","type":"text"},{"code":"Optional<Flavor>","type":"codeVoice"},{"text":"type. Since the","type":"text"},{"text":" ","type":"text"},{"text":"tag modifier defaults to having ","type":"text"},{"code":"includeOptional","type":"codeVoice"},{"text":" enabled, even though","type":"text"},{"text":" ","type":"text"},{"text":"the tag for each option is a non-optional ","type":"text"},{"code":"Flavor","type":"codeVoice"},{"text":", the tag modifier","type":"text"},{"text":" ","type":"text"},{"text":"writes values for both the non-optional, and optional versions of the","type":"text"},{"text":" ","type":"text"},{"text":"value, allowing the contents to be selectable by the ","type":"text"},{"code":"Picker","type":"codeVoice"},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"A "},{"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ForEach","isActive":true,"type":"reference"},{"type":"text","text":" automatically applies a default tag to each enumerated"},{"type":"text","text":" "},{"type":"text","text":"view using the "},{"code":"id","type":"codeVoice"},{"type":"text","text":" parameter of the corresponding element. If"},{"type":"text","text":" "},{"type":"text","text":"the element’s "},{"code":"id","type":"codeVoice"},{"type":"text","text":" parameter and the picker’s "},{"code":"selection","type":"codeVoice"},{"type":"text","text":" input"},{"type":"text","text":" "},{"type":"text","text":"have exactly the same type, or the same type but optional, you can omit"},{"text":" ","type":"text"},{"text":"the explicit tag modifier.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"To see examples that don’t require an explicit tag, see "},{"type":"codeVoice","code":"Picker"},{"type":"text","text":"."}]}],"kind":"content"}],"variants":[{"paths":["\/documentation\/openswiftui\/zstack\/tag(_:includeoptional:)"],"traits":[{"interfaceLanguage":"swift"}]}],"references":{"https://developer.apple.com/documentation/swift/hashable":{"title":"Hashable","titleInlineContent":[{"text":"Hashable","type":"text"}],"url":"https:\/\/developer.apple.com\/documentation\/swift\/hashable","type":"link","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/hashable"},"doc://OpenSwiftUI/documentation/OpenSwiftUI/ZStack":{"title":"ZStack","url":"\/documentation\/openswiftui\/zstack","role":"symbol","navigatorTitle":[{"text":"ZStack","kind":"identifier"}],"kind":"symbol","identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/ZStack","abstract":[{"text":"A view that overlays its subviews, aligning them in both axes.","type":"text"}],"type":"topic","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"ZStack","kind":"identifier"}]},"doc://OpenSwiftUI/documentation/OpenSwiftUI/ForEach":{"url":"\/documentation\/openswiftui\/foreach","title":"ForEach","abstract":[{"text":"A structure that computes views on demand from an underlying collection of","type":"text"},{"text":" ","type":"text"},{"text":"identified data.","type":"text"}],"kind":"symbol","fragments":[{"text